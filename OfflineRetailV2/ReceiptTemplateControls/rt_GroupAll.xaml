<UserControl x:Class="OfflineRetailV2.ReceiptTemplateControls.rt_GroupAll"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OfflineRetailV2.ReceiptTemplateControls"
             mc:Ignorable="d" 
             Width="350" Margin="0,5" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="5"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Image x:Name="imgUp" Grid.Row="0" Grid.Column="1" Source="{DynamicResource Expand}" Height="20" Width="20" HorizontalAlignment="Right" Visibility="Collapsed" Margin="2,2,25,2"></Image>
        <Image x:Name="imgDown" Grid.Row="4" Grid.Column="1" Source="{DynamicResource Expand}" Height="20" Width="20" HorizontalAlignment="Right" Visibility="Collapsed" Margin="2,2,25,2"></Image>
        <Image x:Name="imgEdit"  Grid.Row="1" Grid.Column="0" Source="{DynamicResource ItemMenu}" Height="24" Width="24" HorizontalAlignment="Left" ></Image>
        <Image x:Name="imgAdd"  Grid.Row="1" Grid.Column="1" Source="{DynamicResource Expand}" Height="24" Width="24" HorizontalAlignment="Right"  Margin="2,2,25,2" ></Image>
        <Rectangle Grid.Row="2" Height="1" Grid.Column="0" Grid.ColumnSpan="2" Fill="Black"></Rectangle>
        <WrapPanel x:Name="pnlData" Grid.Row="3" Orientation="Vertical" Grid.Column="0" Grid.ColumnSpan="2">
            
        </WrapPanel>
    </Grid>
</UserControl>
