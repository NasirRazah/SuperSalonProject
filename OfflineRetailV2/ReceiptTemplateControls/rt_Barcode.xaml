<UserControl x:Class="OfflineRetailV2.ReceiptTemplateControls.rt_Barcode"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OfflineRetailV2.ReceiptTemplateControls"
             xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
             mc:Ignorable="d" 
             Width="350"  Padding="2,2,15,2">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"></RowDefinition>
        </Grid.RowDefinitions>
        <dxe:BarCodeEdit x:Name="imgBarcode" EditValue="67152" ShowText="False">
            <dxe:BarCodeEdit.StyleSettings>
                <dxe:Code128StyleSettings ></dxe:Code128StyleSettings>
            </dxe:BarCodeEdit.StyleSettings>
        </dxe:BarCodeEdit>
    </Grid>
</UserControl>
