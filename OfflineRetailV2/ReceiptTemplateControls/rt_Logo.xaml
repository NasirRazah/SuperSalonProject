<UserControl x:Class="OfflineRetailV2.ReceiptTemplateControls.rt_Logo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OfflineRetailV2.ReceiptTemplateControls"
             mc:Ignorable="d" 
             Width="350"  Padding="2,2,15,2">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"></RowDefinition>
        </Grid.RowDefinitions>
        <Image x:Name="imglogo" Height="45" Width="45" Source="{DynamicResource Logo}" HorizontalAlignment="Left"></Image>
    </Grid>
</UserControl>
