            <Setter.Value>
                <ControlTemplate TargetType="Button" x:Name="ctemp">
                    <Border x:Name="bd" Background="#292F41" BorderThickness="0,1,0,0">
                        <Border.BorderBrush>
                            <SolidColorBrush Color="#FF6EC1DA" Opacity="0.9"/>
                        </Border.BorderBrush>
                        <ContentPresenter VerticalAlignment="Center" HorizontalAlignment="Center" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsFocused" Value="True">
                            <Setter Property="BorderBrush" TargetName="bd" Value="#2370C6DD"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

