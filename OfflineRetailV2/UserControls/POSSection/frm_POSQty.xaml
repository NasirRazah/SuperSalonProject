<Window x:Class="OfflineRetailV2.UserControls.POSSection.frm_POSQty"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OfflineRetailV2.UserControls.POSSection"
       xmlns:cc="clr-namespace:OfflineRetailV2.Controls"
        xmlns:lm="clr-namespace:OfflineRetailV2.LayoutManager"
        xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
        xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid" 
        xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
        AllowsTransparency="True" WindowStyle="None" Background="Transparent" WindowStartupLocation="CenterScreen"
        Title="" Foreground="White"   Width="380" Height="300">
    <cc:ModalWindow  x:Name="ModalWindow">
        <Grid Margin="9">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="9"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
            </Grid.RowDefinitions>
            <Grid Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1.5*"></ColumnDefinition>
                    <ColumnDefinition Width="7"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <TextBlock Grid.Column="0" x:Name="lbQty" TextAlignment="Center" FontFamily="{StaticResource OSRegular}" FontSize="20"  Text="1"></TextBlock>
                <Button Grid.Column="2" Content="Cancel" x:Name="btnKeyCancel" Style="{DynamicResource PadButtonStyle}" Height="50" Click="BtnKeyCancel_Click"></Button>
            </Grid>
        <Grid Grid.Row="2">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="7"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="7"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="9"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="9"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="9"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Button Grid.Row="0" Grid.Column="0" x:Name="btnKey1" Focusable="False" Height="50" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Content="1" FontSize="15"
                                Style="{DynamicResource PadButtonStyle}" Click="btnKey1_Click"/>
            <Button Grid.Row="0" Grid.Column="2" x:Name="btnKey2" Focusable="False" Height="50" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Content="2" FontSize="15"
                                Style="{DynamicResource PadButtonStyle}" Click="btnKey1_Click"/>
            <Button Grid.Row="0" Grid.Column="4" x:Name="btnKey3"  Focusable="False" Height="50" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Content="3" FontSize="15"
                                Style="{DynamicResource PadButtonStyle}" Click="btnKey1_Click"/>
            <Button x:Name="btnKey0" Grid.Row="0" Grid.Column="6"  Focusable="False" Height="50" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Content="0"
                                FontSize="15" Style="{DynamicResource PadButtonStyle}" Click="btnKey1_Click"/>

            <Button Grid.Row="2" Grid.Column="0" x:Name="btnKey4" Focusable="False"  Height="50" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Content="4" FontSize="15"
                                Style="{DynamicResource PadButtonStyle}" Click="btnKey1_Click"/>
            <Button Grid.Row="2" Grid.Column="2" x:Name="btnKey5" Focusable="False"  Height="50" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Content="5" FontSize="15"
                                Style="{DynamicResource PadButtonStyle}" Click="btnKey1_Click"/>
            <Button Grid.Row="2" Grid.Column="4" x:Name="btnKey6" Focusable="False" Height="50" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Content="6" FontSize="15"
                                Style="{DynamicResource PadButtonStyle}" Click="btnKey1_Click"/>

            <Button Grid.Row="4" Grid.Column="0" x:Name="btnKey7" Focusable="False"  Height="50" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Content="7" FontSize="15"
                                Style="{DynamicResource PadButtonStyle}" Click="btnKey1_Click"/>
            <Button Grid.Row="4" Grid.Column="2"  x:Name="btnKey8" Focusable="False" Height="50" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Content="8" FontSize="15"
                                Style="{DynamicResource PadButtonStyle}" Click="btnKey1_Click"/>
            <Button Grid.Row="4" Grid.Column="4" x:Name="btnKey9" Focusable="False"  Height="50" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Content="9" FontSize="15"
                                Style="{DynamicResource PadButtonStyle}" Click="btnKey1_Click"/>

            <Button x:Name="btnKeyEnter" Grid.Row="2" Grid.RowSpan="3" Focusable="False" Grid.Column="6" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Content="Enter"
                                FontSize="11" FontFamily="{StaticResource OSSemiBold}" Style="{DynamicResource PadButtonStyle}"
                                        Click="btnKeyEnter_Click"/>
        </Grid>
        </Grid>
    </cc:ModalWindow>
</Window>
