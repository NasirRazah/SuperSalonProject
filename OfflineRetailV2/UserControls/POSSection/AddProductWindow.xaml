<Window x:Class="OfflineRetailV2.UserControls.POSSection.AddProductWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:OfflineRetailV2.UserControls.POSSection"
        xmlns:cc="clr-namespace:OfflineRetailV2.Controls"
        xmlns:lm="clr-namespace:OfflineRetailV2.LayoutManager"
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"  
        xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid" 
        xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
        AllowsTransparency="True" WindowStyle="None" Background="Transparent" WindowStartupLocation="CenterScreen"
        Title="" Foreground="White" Width="1024" Height="740" Loaded="Window_Loaded" Closing="Window_Closing">
    <cc:ModalWindow x:Name="ModalWindow">
        <Grid Margin="9">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>

            </Grid.RowDefinitions>
            <Grid Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <TextBlock Text="Title"  x:Name="Title" VerticalAlignment="Center" Style="{DynamicResource BlueHeading}" />
                <Grid Grid.Column="1" HorizontalAlignment="Right" Margin="0,0,20,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                        <ColumnDefinition Width="9"></ColumnDefinition>
                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                        <ColumnDefinition Width="9"></ColumnDefinition>
                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Button Content="OK" Grid.Column="0" Height="35" Width="250" FontFamily="{StaticResource OSSemiBold}" 
                        Style="{DynamicResource GeneralButtonStyle}" x:Name="btnProductType" Visibility="Hidden" Click="BtnProductType_Click"/>
                    <Button Grid.Column="2" Content="CANCEL" Height="35" Width="120" FontFamily="{StaticResource OSSemiBold}" 
                        Style="{DynamicResource CancelButtonStyle}" x:Name="btnCancel" Click="BtnCancel_Click" />
                    <Button Grid.Column="4" Content="SAVE" Height="35" Width="120" FontFamily="{StaticResource OSSemiBold}" 
                        Style="{DynamicResource SaveButtonStyle}" x:Name="btnLogin" Click="BtnLogin_Click" VerticalAlignment="Top"/>
                </Grid>
            </Grid>
            
            <UniformGrid Columns="6" >
                <FrameworkElement x:Name="c1"/>
            </UniformGrid>
            <TabControl Style="{DynamicResource OuterPOSTabControlStyle}" x:Name="tcProduct" Grid.Row="2" MouseLeftButtonUp="TcProduct_MouseLeftButtonUp">
                <TabItem  Width="{Binding ElementName=c1, Path=ActualWidth}" x:Name="tpGeneral"
                         Style="{DynamicResource OuterPOSTabItemStyle}"  PreviewMouseLeftButtonUp="TpGeneral_PreviewMouseLeftButtonUp">
                    <TabItem.Header>
                        <TextBlock Text="General Details" Style="{DynamicResource TabItemText}"></TextBlock>
                    </TabItem.Header>
                    <Grid Margin="9">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="2"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="2"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>

                        <Grid Grid.Column="0" Margin="7,0,7,0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="9"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="5"></ColumnDefinition>
                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                            </Grid.ColumnDefinitions>

                            <Border Grid.Row="0" Grid.ColumnSpan="3" Style="{DynamicResource GreenBorder}">
                                <TextBlock Style="{DynamicResource GreenBorderInfo}"  FontSize="13"  Text="Product Description"></TextBlock>
                            </Border>

                            <TextBlock Grid.Row="2" Text="SKU" FontSize="12" FontFamily="{StaticResource OSSemiBold}"/>
                            <dxe:TextEdit Grid.Row="4"   HorizontalAlignment="Stretch" x:Name="txtSKU" MaxLength="16" EditValueChanged="TxtSKU_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus"/>

                            <TextBlock Grid.Row="6" Text="Description" FontSize="12" FontFamily="{StaticResource OSSemiBold}"/>
                            <dxe:TextEdit Grid.Row="8"   HorizontalAlignment="Stretch" x:Name="txtDesc" MaxLength="150" EditValueChanged="TxtSKU_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus"/>

                            <TextBlock Grid.Row="10" Text="Product Type" FontSize="12" FontFamily="{StaticResource OSSemiBold}"/>

                            <dxe:ComboBoxEdit  Grid.Row="12" HorizontalAlignment="Stretch"
                                          x:Name="cmbProductType" SelectedIndex="-1" IsTextEditable="False" SelectedIndexChanged="CmbProductType_SelectedIndexChanged" PreviewMouseLeftButtonDown="CmbProductType_PreviewMouseLeftButtonDown">
                                <dxe:ComboBoxEditItem>Product</dxe:ComboBoxEditItem>
                                <dxe:ComboBoxEditItem>Unit Of Measure</dxe:ComboBoxEditItem>
                                <dxe:ComboBoxEditItem>Matrix</dxe:ComboBoxEditItem>
                                <dxe:ComboBoxEditItem>Serialized</dxe:ComboBoxEditItem>
                                <dxe:ComboBoxEditItem>Kit</dxe:ComboBoxEditItem>
                                <dxe:ComboBoxEditItem>Fuel</dxe:ComboBoxEditItem>
                                <dxe:ComboBoxEditItem>Weighted</dxe:ComboBoxEditItem>
                                <dxe:ComboBoxEditItem>Donation</dxe:ComboBoxEditItem>
                                <dxe:ComboBoxEditItem>Entry Ticket</dxe:ComboBoxEditItem>
                            </dxe:ComboBoxEdit>

                            <Grid Grid.Row="14" Grid.ColumnSpan="3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                    <ColumnDefinition Width="5"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Text="Department" FontSize="12" FontFamily="{StaticResource OSSemiBold}"/>
                                <dxe:HyperlinkEdit FontSize="11" HorizontalAlignment="Right" AllowAutoNavigate="True" Grid.Column="2" Text="changed to weighed" x:Name="lnkType" RequestNavigation="LnkType_RequestNavigation" Margin="0,0,30,0"></dxe:HyperlinkEdit>
                            </Grid>

                            <dxg:LookUpEdit Grid.Row="16"  x:Name="cmbDept" HorizontalAlignment="Stretch" ItemsSource="{Binding}"
                                        DisplayMember="Description"
                                        ValueMember="ID"
                                        AutoPopulateColumns="False"
                                        AutoComplete="True"
                                        IncrementalFiltering="True"
                                        ImmediatePopup="True"
                                        IsPopupAutoWidth="False"
                                         EditValueChanged="TxtSKU_EditValueChanged" PreviewMouseLeftButtonDown="CmbDept_PreviewMouseLeftButtonDown" 
                                            PopupOpened="gen_PopupOpened">
                                <dxg:LookUpEdit.PopupContentTemplate>
                                    <ControlTemplate>
                                        <dxg:GridControl Name="PART_GridControl" AutoGenerateColumns="None">
                                            <dxg:GridControl.Columns>
                                                <dxg:GridColumn FieldName="Image" Visible="False" VisibleIndex="-1" Width="*" AllowAutoFilter="False" Focusable="False">
                                                    <dxg:GridColumn.EditSettings>
                                                        <dxe:ImageEditSettings Stretch="UniformToFill"/>
                                                    </dxg:GridColumn.EditSettings>
                                                </dxg:GridColumn>
                                                <dxg:GridColumn FieldName="ID" Visible="False" />
                                                <dxg:GridColumn FieldName="DepartmentID" Header="Department ID" VisibleIndex="1" />
                                                <dxg:GridColumn FieldName="Description" Header="Description" VisibleIndex="2" />
                                            </dxg:GridControl.Columns>
                                            <dxg:GridControl.View>
                                                <dxg:TableView AutoWidth="True" />
                                            </dxg:GridControl.View>
                                        </dxg:GridControl>
                                    </ControlTemplate>
                                </dxg:LookUpEdit.PopupContentTemplate>
                            </dxg:LookUpEdit>

                            <Image Grid.Row="16" Grid.Column="2" 
                                       Source="{DynamicResource GreenAdd}"  x:Name="btnDAdd"  PreviewMouseLeftButtonDown="btnDAdd_PreviewMouseLeftButtonDown" >
                            </Image>


                            <TextBlock Grid.Row="18" Text="POS Screen Category" FontSize="12" FontFamily="{StaticResource OSSemiBold}"/>
                            <dxg:LookUpEdit Grid.Row="20"   x:Name="cmbCategory" HorizontalAlignment="Stretch" ItemsSource="{Binding}"
                                    DisplayMember="Description"
                                        ValueMember="ID"
                                        AutoPopulateColumns="False"
                                        AutoComplete="True"
                                        IncrementalFiltering="True"
                                        ImmediatePopup="True"
                                        
                                        IsPopupAutoWidth="False" EditValueChanged="CmbCategory_EditValueChanged" PreviewMouseLeftButtonDown="CmbCategory_PreviewMouseLeftButtonDown" PopupOpened="CmbCategory_PopupOpened"
                                        >
                                <dxg:LookUpEdit.PopupContentTemplate>
                                    <ControlTemplate>
                                        <dxg:GridControl Name="PART_GridControl" AutoGenerateColumns="None">
                                            <dxg:GridControl.Columns>
                                                <dxg:GridColumn FieldName="Image" Visible="False" VisibleIndex="-1" Width="*" AllowAutoFilter="False" Focusable="False">
                                                    <dxg:GridColumn.EditSettings>
                                                        <dxe:ImageEditSettings Stretch="UniformToFill"/>
                                                    </dxg:GridColumn.EditSettings>
                                                </dxg:GridColumn>
                                                <dxg:GridColumn FieldName="ID" Visible="False" />
                                                <dxg:GridColumn FieldName="CategoryID" Header="Category ID" VisibleIndex="1" />
                                                <dxg:GridColumn FieldName="Description" Header="Description" VisibleIndex="2" />
                                            </dxg:GridControl.Columns>
                                            <dxg:GridControl.View>
                                                <dxg:TableView AutoWidth="True" />
                                            </dxg:GridControl.View>
                                        </dxg:GridControl>
                                    </ControlTemplate>
                                </dxg:LookUpEdit.PopupContentTemplate>
                                
                            </dxg:LookUpEdit>
                            <Image Grid.Row="20" Grid.Column="2"  x:Name="btnCAdd"
                                       Source="{DynamicResource GreenAdd}" PreviewMouseLeftButtonDown="btnCAdd_PreviewMouseLeftButtonDown">
                                
                            </Image>


                            <TextBlock Grid.Row="22" Text="Family" FontSize="12" FontFamily="{StaticResource OSSemiBold}"/>
                            <dxg:LookUpEdit Grid.Row="24"  x:Name="cmbBrand" HorizontalAlignment="Stretch" ItemsSource="{Binding}"
                                    DisplayMember="BrandDescription"
                                        ValueMember="ID"
                                        AutoPopulateColumns="False"
                                        AutoComplete="True"
                                        IncrementalFiltering="True"
                                        ImmediatePopup="True"
                                        IsPopupAutoWidth="False"
                                        EditValueChanged="TxtSKU_EditValueChanged" PreviewMouseLeftButtonDown="CmbBrand_PreviewMouseLeftButtonDown"
                                            PopupOpened="gen_PopupOpened">
                                <dxg:LookUpEdit.PopupContentTemplate>
                                    <ControlTemplate>
                                        <dxg:GridControl Name="PART_GridControl" AutoGenerateColumns="None">
                                            <dxg:GridControl.Columns>
                                                <dxg:GridColumn FieldName="Image" Visible="False" VisibleIndex="-1" Width="*" AllowAutoFilter="False" Focusable="False">
                                                    <dxg:GridColumn.EditSettings>
                                                        <dxe:ImageEditSettings Stretch="UniformToFill"/>
                                                    </dxg:GridColumn.EditSettings>
                                                </dxg:GridColumn>
                                                <dxg:GridColumn FieldName="ID" Visible="False" />
                                                <dxg:GridColumn FieldName="BrandID" Header="Family ID" VisibleIndex="1" />
                                                <dxg:GridColumn FieldName="BrandDescription" Header="Family Name" VisibleIndex="2" />
                                            </dxg:GridControl.Columns>
                                            <dxg:GridControl.View>
                                                <dxg:TableView AutoWidth="True" />
                                            </dxg:GridControl.View>
                                        </dxg:GridControl>
                                    </ControlTemplate>
                                </dxg:LookUpEdit.PopupContentTemplate>
                            </dxg:LookUpEdit>

                            <TextBlock Grid.Row="26" Text="Alt SKU" FontSize="12" FontFamily="{StaticResource OSSemiBold}" x:Name="lbAltSKU"/>
                            <dxe:TextEdit Grid.Row="28"   x:Name="txtAltSKU" MaxLength="16" EditValueChanged="TxtSKU_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus"/>

                            <TextBlock Grid.Row="30" Text="Alt SKU2" FontSize="12" FontFamily="{StaticResource OSSemiBold}" x:Name="lbAltSKU2"/>
                            <dxe:TextEdit Grid.Row="32"   x:Name="txtAltSKU2" MaxLength="16" EditValueChanged="TxtSKU_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus"/>

                            <TextBlock Grid.Row="34" Text="UPC" FontSize="12" FontFamily="{StaticResource OSSemiBold}" x:Name="lbUPC"/>
                            <dxe:TextEdit Grid.Row="36"   x:Name="txtUPC" MaxLength="20" EditValueChanged="TxtSKU_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus"/>

                            <TextBlock Grid.Row="38" Text="Expiry Date" FontSize="12" FontFamily="{StaticResource OSSemiBold}" x:Name="lbExpiry"/>
                            <dxe:DateEdit Grid.Row="40" x:Name="dtExpiry" EditValueChanged="TxtSKU_EditValueChanged" PreviewMouseLeftButtonDown="DtExpiry_PreviewMouseLeftButtonDown"/>
                        </Grid>
                        <Grid Grid.Column="1" Style="{DynamicResource ScreenSeparator}"></Grid>
                        <Grid Grid.Column="2" Margin="7,0,7,0">

                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="9"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="15"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="9"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="5"></ColumnDefinition>
                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                            </Grid.ColumnDefinitions>

                            <Border Grid.Row="0" Grid.ColumnSpan="3" Style="{DynamicResource OrangeBorder}">
                                <TextBlock Style="{DynamicResource OrangeBorderInfo}"  FontSize="13" Text="Finances"></TextBlock>
                            </Border>

                            <TextBlock Grid.Row="2" Text="Original Cost" FontSize="12" FontFamily="{StaticResource OSSemiBold}"/>
                            <dxe:TextEdit Grid.Row="4"   HorizontalAlignment="Stretch" x:Name="numCcost" Mask="f2" MaskType="Numeric" MaskUseAsDisplayFormat="True"  Text="0.00" EditValueChanged="NumCcost_EditValueChanged" GotFocus="Num_GotFocus" LostFocus="Num_LostFocus"/>

                            <TextBlock Grid.Row="6" Text="Discounted Cost" FontSize="12" FontFamily="{StaticResource OSSemiBold}"/>
                            <dxe:TextEdit Grid.Row="8"   HorizontalAlignment="Stretch" x:Name="numDcost" Mask="f2" MaskType="Numeric" MaskUseAsDisplayFormat="True"  Text="0.00" EditValueChanged="NumCcost_EditValueChanged" GotFocus="Num_GotFocus" LostFocus="Num_LostFocus"/>

                            <TextBlock Grid.Row="10" Text="Last Purchase" FontSize="12" FontFamily="{StaticResource OSSemiBold}"/>
                            <dxe:TextEdit Grid.Row="12"   HorizontalAlignment="Stretch" x:Name="numLPcost" Mask="f2" MaskType="Numeric" MaskUseAsDisplayFormat="True" Text="0.00" GotFocus="Num_GotFocus" LostFocus="Num_LostFocus"/>

                            <TextBlock Grid.Row="14"  Text="Price A" FontSize="12" FontFamily="{StaticResource OSSemiBold}"/>
                            <dxe:TextEdit Grid.Row="16"  HorizontalAlignment="Stretch" x:Name="numPriceA" Mask="f2" MaskType="Numeric" MaskUseAsDisplayFormat="True" Text="0.00" EditValueChanged="NumCcost_EditValueChanged" GotFocus="Num_GotFocus" LostFocus="Num_LostFocus"/>
                            <TextBlock Grid.Row="16" Grid.Column="2" Text="N/A" FontSize="11" FontFamily="{StaticResource OSSemiBold}" x:Name="lbPriceMarginA"/>

                            <TextBlock Grid.Row="18"  Text="Price B" FontSize="12" FontFamily="{StaticResource OSSemiBold}"/>
                            <dxe:TextEdit Grid.Row="20"   HorizontalAlignment="Stretch" x:Name="numPriceB" Mask="f2" MaskType="Numeric" MaskUseAsDisplayFormat="True"  Text="0.00" EditValueChanged="NumCcost_EditValueChanged" GotFocus="Num_GotFocus" LostFocus="Num_LostFocus"/>
                            <TextBlock Grid.Row="20" Grid.Column="2" Text="N/A" FontSize="11" FontFamily="{StaticResource OSSemiBold}" x:Name="lbPriceMarginB"/>

                            <TextBlock Grid.Row="22"  Text="Price C" FontSize="12" FontFamily="{StaticResource OSSemiBold}"/>
                            <dxe:TextEdit Grid.Row="24"   HorizontalAlignment="Stretch" x:Name="numPriceC" Mask="f2" MaskType="Numeric" MaskUseAsDisplayFormat="True" Text="0.00" EditValueChanged="NumCcost_EditValueChanged" GotFocus="Num_GotFocus" LostFocus="Num_LostFocus"/>
                            <TextBlock Grid.Row="24" Grid.Column="2" Text="N/A" FontSize="12" FontFamily="{StaticResource OSSemiBold}" x:Name="lbPriceMarginC"/>

                            <Border Grid.Row="26" Grid.ColumnSpan="3" Style="{DynamicResource BlueBorder}">
                                <TextBlock Style="{DynamicResource BlueBorderInfo}"  FontSize="13" Text="Tax"></TextBlock>
                            </Border>

                            <dxg:GridControl Grid.Row="28" Grid.ColumnSpan="3" ShowBorder="True" x:Name="grdTax" Height="127"  VerticalAlignment="Top" HorizontalAlignment="Left"  AutoGenerateColumns="AddNew" EnableSmartColumnsGeneration="False"  PreviewMouseLeftButtonDown="GrdTax_PreviewMouseLeftButtonDown">
                                <dxg:GridControl.View>
                                    <dxg:TableView NavigationStyle="Cell" AllowPerPixelScrolling="True" ShowTotalSummary="False" x:Name="gridView3" ShowFilterPanelMode="Never" AllowColumnFiltering="False" CellValueChanging="GridView3_CellValueChanging"/>
                                </dxg:GridControl.View>
                                <dxg:GridColumn FieldName="ID" Header="ID" Width="*" x:Name="colMappingID" Visible="False"/>
                                <dxg:GridColumn FieldName="Image" Visible="False" VisibleIndex="-1" Width="*" AllowAutoFilter="False" Focusable="False">
                                    <dxg:GridColumn.EditSettings>
                                        <dxe:ImageEditSettings Stretch="UniformToFill"/>
                                    </dxg:GridColumn.EditSettings>
                                </dxg:GridColumn>
                                <dxg:GridColumn IsSmart="False" FieldName="TaxID" Header="Tax ID" Width="100" x:Name="colTaxID" AllowFocus="True" AllowEditing="True">
                                    <dxg:GridColumn.EditSettings>
                                        <dxg:LookUpEditSettings  x:Name="PART_Editor_Tax" ValueMember="TaxID" DisplayMember="TaxID" ImmediatePopup="True" AutoComplete="False" IsPopupAutoWidth="True" PreviewMouseLeftButtonDown="PART_Editor_Tax_PreviewMouseLeftButtonDown" >
                                            <dxg:LookUpEditSettings.PopupContentTemplate>
                                                <ControlTemplate>
                                                    <dxg:GridControl  Style="{DynamicResource partGrd}" x:Name="PART_GridControl" EnableSmartColumnsGeneration="False" ShowBorder="False" BorderBrush="Black" VerticalAlignment="Stretch" VerticalContentAlignment="Stretch" >
                                                        <dxg:GridControl.View>
                                                            <dxg:TableView x:Name="gridView2" AutoWidth="True" NavigationStyle="Row" Background="AliceBlue" ShowTotalSummary="false" AllowEditing="False" ShowIndicator="False" ShowFilterPanelMode="Never" ShowGroupPanel="False" FadeSelectionOnLostFocus="False" ShowVerticalLines="False"  AllowColumnFiltering="False"  >
                                                            </dxg:TableView>
                                                        </dxg:GridControl.View>
                                                        <dxg:GridControl.Columns>
                                                            <dxg:GridColumn FieldName="Image" Visible="False" VisibleIndex="-1" Width="*" AllowAutoFilter="False" Focusable="False">
                                                                <dxg:GridColumn.EditSettings>
                                                                    <dxe:ImageEditSettings Stretch="UniformToFill"/>
                                                                </dxg:GridColumn.EditSettings>
                                                            </dxg:GridColumn>
                                                            <dxg:GridColumn IsSmart="False" FieldName="ID" Header="ID" Visible="False" VisibleIndex="-1"   />
                                                            <dxg:GridColumn IsSmart="False" FieldName="TaxID" Header="Tax ID" Visible="False" VisibleIndex="-1"   />
                                                            <dxg:GridColumn IsSmart="False" FieldName="TaxName" Header="Tax" Visible="True" VisibleIndex="1"/>
                                                        </dxg:GridControl.Columns>
                                                    </dxg:GridControl>
                                                </ControlTemplate>
                                            </dxg:LookUpEditSettings.PopupContentTemplate>
                                        </dxg:LookUpEditSettings>

                                    </dxg:GridColumn.EditSettings>
                                </dxg:GridColumn>
                                <dxg:GridColumn IsSmart="False" FieldName="TaxName"  Width="*" x:Name="colTaxName"  AllowEditing="False" AllowFocus="False" HorizontalHeaderContentAlignment="Stretch" AllowSorting="False" >
                                    <dxg:GridColumn.Header>
                                        <Grid Margin="0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="40"/>
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="25"></RowDefinition>
                                            </Grid.RowDefinitions>
                                            <TextBlock Grid.Column="0" Text="Name" Foreground="White" FontFamily="{DynamicResource OSSemiBold}" />
                                            <Button Grid.Column="1" Style="{DynamicResource DeleteGridRowButton}" Margin="0,-9" Background="Transparent" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" BorderBrush="Transparent" BorderThickness="0"  x:Name="btnDelTax" HorizontalAlignment="Right" Click="BtnDelTax_Click" Width="30" >

                                            </Button>

                                        </Grid>
                                    </dxg:GridColumn.Header>
                                </dxg:GridColumn>
                            </dxg:GridControl>

                        </Grid>
                        <Grid Grid.Column="3" Style="{DynamicResource ScreenSeparator}"></Grid>
                        <Grid Grid.Column="4" Margin="7,0,7,0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="9"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="3"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="15"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="9"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="15"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="9"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="30"></ColumnDefinition>
                            </Grid.ColumnDefinitions>

                            <Border Grid.Row="0" Grid.ColumnSpan="3" Style="{DynamicResource PurpleBorder}">
                                <TextBlock Style="{DynamicResource PurpleBorderInfo}"  FontSize="13"  Text="Stock"></TextBlock>
                            </Border>


