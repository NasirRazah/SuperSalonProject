<Window x:Class="OfflineRetailV2.UserControls.POSSection.frm_ServiceDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:OfflineRetailV2.UserControls.POSSection"
        xmlns:cc="clr-namespace:OfflineRetailV2.Controls"
        xmlns:lm="clr-namespace:OfflineRetailV2.LayoutManager"
           xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
         xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
         xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid" 
            xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
        AllowsTransparency="True" WindowStyle="None" Background="Transparent" WindowStartupLocation="CenterScreen"
        Title="" Foreground="White"   
        Width="1024" Height="740" Closing="Window_Closing" Loaded="Window_Loaded">
    <cc:ModalWindow  x:Name="ModalWindow">
        <Grid Margin="9">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <TextBlock Text="Title"  x:Name="Title" VerticalAlignment="Center" Style="{DynamicResource BlueHeading}" />
                <Grid Grid.Column="1" HorizontalAlignment="Right" Margin="0,0,20,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                        <ColumnDefinition Width="9"></ColumnDefinition>
                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                    </Grid.ColumnDefinitions>

                    <Button Grid.Column="0" Content="CANCEL" Height="35" Width="120" FontFamily="{StaticResource OSSemiBold}" 
                        Style="{DynamicResource CancelButtonStyle}" x:Name="btnCancel" Click="BtnCancel_Click" />
                    <Button Grid.Column="2" Content="SAVE" Height="35" Width="120" FontFamily="{StaticResource OSSemiBold}" 
                        Style="{DynamicResource SaveButtonStyle}" x:Name="btnLogin" Click="BtnLogin_Click"/>
                </Grid>
            </Grid>
            <Grid Grid.Row="2" Margin="0,9,0,0">
                <UniformGrid Columns="3" >
                    <FrameworkElement x:Name="c1"/>
                </UniformGrid>
                <TabControl Style="{DynamicResource OuterPOSTabControlStyle}" x:Name="tcProduct" MouseLeftButtonUp="TcProduct_MouseLeftButtonUp">
                    <TabItem  Width="{Binding ElementName=c1, Path=ActualWidth}" x:Name="tpGeneral"
                         Style="{DynamicResource OuterPOSTabItemStyle}" PreviewMouseLeftButtonUp="TpGeneral_PreviewMouseLeftButtonUp">
                        <TabItem.Header>
                            <TextBlock Text="General Details" Style="{DynamicResource TabItemText}"></TextBlock>
                        </TabItem.Header>


                        <Grid Margin="9">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="2"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="2"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>

                            <Grid Grid.Column="0" Margin="7,0,7,0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="9"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="15"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="9"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="5"></ColumnDefinition>
                                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                                </Grid.ColumnDefinitions>

                                <Border Grid.Row="0" Grid.ColumnSpan="3" Style="{DynamicResource GreenBorder}">
                                    <TextBlock Style="{DynamicResource GreenBorderInfo}" Text="Service Description"></TextBlock>
                                </Border>


                                <TextBlock Grid.Row="2" Text="Code" FontSize="11" FontFamily="{StaticResource OSSemiBold}"/>
                                <dxe:TextEdit Grid.Row="4"  Height="30" x:Name="txtSKU" MaxLength="16" EditValueChanged="TxtSKU_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus"/>

                                <TextBlock Grid.Row="6" Text="Description" FontSize="11" FontFamily="{StaticResource OSSemiBold}"/>
                                <dxe:TextEdit Grid.Row="8"  Height="30" x:Name="txtDesc" MaxLength="150" EditValueChanged="TxtSKU_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus"/>

                                <TextBlock Grid.Row="10" Text="Department" FontSize="11" FontFamily="{StaticResource OSSemiBold}"/>
                                <dxg:LookUpEdit Grid.Row="12"  Height="30" x:Name="cmbDept"
                                         ItemsSource="{Binding}"
                                        DisplayMember="Description"
                                        ValueMember="ID"
                                        AutoPopulateColumns="False"
                                        AutoComplete="True"
                                        IncrementalFiltering="True"
                                        ImmediatePopup="True"
                                        IsPopupAutoWidth="False" EditValueChanged="TxtSKU_EditValueChanged" PreviewMouseLeftButtonDown="CmbDept_PreviewMouseLeftButtonDown" PopupOpened="gen_PopupOpened"
                                        >
                                    <dxg:LookUpEdit.PopupContentTemplate>
                                        <ControlTemplate>
                                            <dxg:GridControl Name="PART_GridControl" AutoGenerateColumns="None">
                                                <dxg:GridControl.Columns>
                                                    <dxg:GridColumn FieldName="Image" Visible="False" VisibleIndex="-1" Width="*" AllowAutoFilter="False" Focusable="False">
                                                        <dxg:GridColumn.EditSettings>
                                                            <dxe:ImageEditSettings Stretch="UniformToFill"/>
                                                        </dxg:GridColumn.EditSettings>
                                                    </dxg:GridColumn>
                                                    <dxg:GridColumn FieldName="ID" Visible="False" />
                                                    <dxg:GridColumn FieldName="DepartmentID" Header="Department ID" VisibleIndex="1" />
                                                    <dxg:GridColumn FieldName="Description" Header="Description" VisibleIndex="2" />
                                                </dxg:GridControl.Columns>
                                                <dxg:GridControl.View>
                                                    <dxg:TableView AutoWidth="True" />
                                                </dxg:GridControl.View>
                                            </dxg:GridControl>
                                        </ControlTemplate>
                                    </dxg:LookUpEdit.PopupContentTemplate>
                                </dxg:LookUpEdit>
                                <Image Grid.Row="12" Grid.Column="2"
                                       Source="{DynamicResource GreenAdd}" x:Name="btnDAdd" PreviewMouseLeftButtonDown="btnDAdd_PreviewMouseLeftButtonDown">
                                    
                                </Image>

                                <TextBlock Grid.Row="14" Text="POS Screen Category" FontSize="11" FontFamily="{StaticResource OSSemiBold}"/>
                                <dxg:LookUpEdit Grid.Row="16"  Height="30" x:Name="cmbCategory"
                                         ItemsSource="{Binding}"
                                    DisplayMember="Description"
                                        ValueMember="ID"
                                        AutoPopulateColumns="False"
                                        AutoComplete="True"
                                        IncrementalFiltering="True"
                                        ImmediatePopup="True"
                                        IsPopupAutoWidth="False" PopupOpened="gen_PopupOpened" EditValueChanged="CmbCategory_EditValueChanged" PreviewMouseLeftButtonDown="CmbCategory_PreviewMouseLeftButtonDown" 
                                        >
                                    <dxg:LookUpEdit.PopupContentTemplate>
                                        <ControlTemplate>
                                            <dxg:GridControl Name="PART_GridControl" AutoGenerateColumns="None">
                                                <dxg:GridControl.Columns>
                                                    <dxg:GridColumn FieldName="Image" Visible="False" VisibleIndex="-1" Width="*" AllowAutoFilter="False" Focusable="False">
                                                        <dxg:GridColumn.EditSettings>
                                                            <dxe:ImageEditSettings Stretch="UniformToFill"/>
                                                        </dxg:GridColumn.EditSettings>
                                                    </dxg:GridColumn>
                                                    <dxg:GridColumn FieldName="ID" Visible="False" />
                                                    <dxg:GridColumn FieldName="CategoryID" Header="Category ID" VisibleIndex="1" />
                                                    <dxg:GridColumn FieldName="Description" Header="Description" VisibleIndex="2" />
                                                </dxg:GridControl.Columns>
                                                <dxg:GridControl.View>
                                                    <dxg:TableView AutoWidth="True" />
                                                </dxg:GridControl.View>
                                            </dxg:GridControl>
                                        </ControlTemplate>
                                    </dxg:LookUpEdit.PopupContentTemplate>
                                </dxg:LookUpEdit>
                                <Image Grid.Row="16" Grid.Column="2" x:Name="btnCAdd"
                                       Source="{DynamicResource GreenAdd}" PreviewMouseLeftButtonDown="btnCAdd_PreviewMouseLeftButtonDown">
                                    
                                </Image>


                                <Border Grid.Row="18" Grid.ColumnSpan="3" Style="{DynamicResource PurpleBorder}">
                                    <TextBlock Style="{DynamicResource PurpleBorderInfo}" Text="Service Time &amp; others"></TextBlock>
                                </Border>


                                <TextBlock Grid.Row="20" Text="Minimum Time"/>
                                <dxe:SpinEdit Grid.Row="22" x:Name="spnMinTime" HorizontalContentAlignment="Center" Mask="N0" MaskType="Numeric" MaskUseAsDisplayFormat="True" Increment="5" MinValue="10" Text="10" IsFloatValue="False" EditValueChanged="TxtSKU_EditValueChanged">
                                    <dxe:SpinEdit.Buttons>
                                        <dxe:ButtonInfoCollection>
                                            <dxe:ButtonInfo Command="{Binding SpinDownCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=dxe:SpinEdit}}">
                                                <dxe:ButtonInfo.Content>
                                                    <dx:DXImage Source="{DynamicResource GreenDropdownD}" Height="15" Width="16"  Margin="5,5">
                                                    </dx:DXImage>
                                                </dxe:ButtonInfo.Content>
                                            </dxe:ButtonInfo>
                                            <dxe:ButtonInfo Command="{Binding SpinUpCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=dxe:SpinEdit}}" IsLeft="True">
                                                <dxe:ButtonInfo.Content>
                                                    <dx:DXImage Source="{DynamicResource GreenDropdownU}" Height="15" Width="16"  Margin="5,5">
                                                    </dx:DXImage>
                                                </dxe:ButtonInfo.Content>
                                            </dxe:ButtonInfo>
                                        </dxe:ButtonInfoCollection>
                                    </dxe:SpinEdit.Buttons>
                                </dxe:SpinEdit>

                                <TextBlock Grid.Row="22" Grid.Column="2" Text="min."/>
                                <TextBlock Grid.Row="24"  Text="Min Age"/>
                                <dxe:TextEdit Grid.Row="26"  x:Name="numMinAge" Mask="N0" MaskType="Numeric" MaskUseAsDisplayFormat="True" Text="0" EditValueChanged="TxtSKU_EditValueChanged" GotFocus="Num_GotFocus" LostFocus="Num_LostFocus"/>

                                <TextBlock Grid.Row="28" Text="Points"/>
                                <dxe:TextEdit Grid.Row="30"  x:Name="txtPoints"  Mask="N0" MaskType="Numeric" MaskUseAsDisplayFormat="True" Text="0" EditValueChanged="TxtSKU_EditValueChanged" GotFocus="Num_GotFocus" LostFocus="Num_LostFocus"/>


                            </Grid>
                            <Grid Grid.Column="1" Style="{DynamicResource ScreenSeparator}"></Grid>
                            <Grid Grid.Column="2" Margin="7,0,7,0">

                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="9"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="15"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="9"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="30"></ColumnDefinition>
                                </Grid.ColumnDefinitions>

                                <Border Grid.Row="0" Grid.ColumnSpan="3" Style="{DynamicResource OrangeBorder}">
                                    <TextBlock Style="{DynamicResource OrangeBorderInfo}" Text="Finances"></TextBlock>
                                </Border>

                                <TextBlock Grid.Row="2" Text="Cost" FontSize="11" FontFamily="{StaticResource OSSemiBold}"/>
                                <dxe:TextEdit Grid.Row="4" Height="30" x:Name="numCcost" Text="0.00" Mask="f2" MaskType="Numeric" MaskUseAsDisplayFormat="True"  EditValueChanged="TxtSKU_EditValueChanged" GotFocus="Num_GotFocus" LostFocus="Num_LostFocus"
/>


                                <TextBlock Grid.Row="6"  Text="Price A" FontSize="11" FontFamily="{StaticResource OSSemiBold}"/>
                                <dxe:TextEdit Grid.Row="8"  Height="30" x:Name="numPriceA" Text="0.00" Mask="f2" MaskType="Numeric" MaskUseAsDisplayFormat="True"  EditValueChanged="TxtSKU_EditValueChanged" GotFocus="Num_GotFocus" LostFocus="Num_LostFocus"
/>

                                <TextBlock Grid.Row="10"  Text="Price B" FontSize="11" FontFamily="{StaticResource OSSemiBold}"/>
                                <dxe:TextEdit Grid.Row="12" Height="30" x:Name="numPriceB" Text="0.00" Mask="f2" MaskType="Numeric" MaskUseAsDisplayFormat="True"  EditValueChanged="TxtSKU_EditValueChanged" GotFocus="Num_GotFocus" LostFocus="Num_LostFocus"
/>

                                <TextBlock Grid.Row="14"  Text="Price C" FontSize="11" FontFamily="{StaticResource OSSemiBold}"/>
                                <dxe:TextEdit Grid.Row="16"  Height="30" x:Name="numPriceC" Text="0.00" Mask="f2" MaskType="Numeric" MaskUseAsDisplayFormat="True"  EditValueChanged="TxtSKU_EditValueChanged" GotFocus="Num_GotFocus" LostFocus="Num_LostFocus"></dxe:TextEdit>

                                <Border Grid.Row="18" Grid.ColumnSpan="3" Style="{DynamicResource BlueBorder}">
                                    <TextBlock Style="{DynamicResource BlueBorderInfo}" Text="Tax"></TextBlock>
                                </Border>


                                <dxg:GridControl ShowBorder="True" x:Name="grdTax" Height="150" Grid.Row="20" Grid.ColumnSpan="2" AutoGenerateColumns="AddNew" EnableSmartColumnsGeneration="False" PreviewMouseLeftButtonDown="GrdTax_PreviewMouseLeftButtonDown" >
                                    <dxg:GridControl.View>
                                        <dxg:TableView NavigationStyle="Cell" AllowPerPixelScrolling="True" ShowTotalSummary="False" x:Name="gridView3" ShowFilterPanelMode="Never" AllowColumnFiltering="False" CellValueChanging="GridView3_CellValueChanging" />
                                    </dxg:GridControl.View>
                                    <dxg:GridColumn FieldName="ID" Header="ID" Width="*" x:Name="colMappingID" Visible="False"/>
                                    <dxg:GridColumn FieldName="Image" Visible="False" VisibleIndex="-1" Width="*" AllowAutoFilter="False" Focusable="False">
                                        <dxg:GridColumn.EditSettings>
                                            <dxe:ImageEditSettings Stretch="UniformToFill"/>
                                        </dxg:GridColumn.EditSettings>
                                    </dxg:GridColumn>
                                    <dxg:GridColumn IsSmart="False" FieldName="TaxID" Header="Tax ID" Width="100" x:Name="colTaxID" AllowFocus="True" AllowEditing="True">
                                        <dxg:GridColumn.EditSettings>
                                            <dxg:LookUpEditSettings  x:Name="PART_Editor_Tax" ValueMember="TaxID" DisplayMember="TaxID" ImmediatePopup="True" AutoComplete="False" IsPopupAutoWidth="True" PreviewMouseLeftButtonDown="PART_Editor_Tax_PreviewMouseLeftButtonDown"  >
                                                <dxg:LookUpEditSettings.PopupContentTemplate>
                                                    <ControlTemplate>
                                                        <dxg:GridControl Style="{DynamicResource partGrd}" x:Name="PART_GridControl" EnableSmartColumnsGeneration="False" ShowBorder="False" BorderBrush="Black" VerticalAlignment="Stretch" VerticalContentAlignment="Stretch" >
                                                            <dxg:GridControl.View>
                                                                <dxg:TableView x:Name="gridView2" AutoWidth="True" NavigationStyle="Row" Background="AliceBlue" ShowTotalSummary="false" AllowEditing="False" ShowIndicator="False" ShowFilterPanelMode="Never" ShowGroupPanel="False" FadeSelectionOnLostFocus="False" ShowVerticalLines="False"  AllowColumnFiltering="False"  >
                                                                </dxg:TableView>
                                                            </dxg:GridControl.View>
                                                            <dxg:GridControl.Columns>
                                                                <dxg:GridColumn FieldName="Image" Visible="False" VisibleIndex="-1" Width="*" AllowAutoFilter="False" Focusable="False">
                                                                    <dxg:GridColumn.EditSettings>
                                                                        <dxe:ImageEditSettings Stretch="UniformToFill"/>
                                                                    </dxg:GridColumn.EditSettings>
                                                                </dxg:GridColumn>
                                                                <dxg:GridColumn IsSmart="False" FieldName="ID" Header="ID" Visible="False" VisibleIndex="-1"   />
                                                                <dxg:GridColumn IsSmart="False" FieldName="TaxID" Header="Tax ID" Visible="False" VisibleIndex="-1"   />
                                                                <dxg:GridColumn IsSmart="False" FieldName="TaxName" Header="Tax" Visible="True" VisibleIndex="1"/>
                                                            </dxg:GridControl.Columns>
                                                        </dxg:GridControl>
                                                    </ControlTemplate>
                                                </dxg:LookUpEditSettings.PopupContentTemplate>
                                            </dxg:LookUpEditSettings>

                                        </dxg:GridColumn.EditSettings>
                                    </dxg:GridColumn>
                                    <dxg:GridColumn FieldName="TaxName"  Width="*" x:Name="colTaxName" AllowEditing="False" AllowFocus="False" HorizontalHeaderContentAlignment="Stretch" AllowSorting="False">
                                        <dxg:GridColumn.Header>
                                            <Grid Margin="0">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*"/>
                                                    <ColumnDefinition Width="40"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="25"></RowDefinition>
                                                </Grid.RowDefinitions>
                                                <TextBlock Grid.Column="0" Text="Name" Foreground="White" FontFamily="{DynamicResource OSSemiBold}" />
                                                <Button Grid.Column="1" Style="{DynamicResource DeleteGridRowButton}" Margin="0,-9" Background="Transparent" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" BorderBrush="Transparent" BorderThickness="0"  x:Name="simpleButton3" HorizontalAlignment="Right" Click="SimpleButton3_Click" Width="30" >

                                                </Button>

                                            </Grid>
                                        </dxg:GridColumn.Header>
                                    </dxg:GridColumn>
                                </dxg:GridControl>

                            </Grid>
                            <Grid Grid.Column="3" Style="{DynamicResource ScreenSeparator}"></Grid>
                            <Grid Grid.Column="4" Margin="7,0,7,0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="9"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="15"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="9"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
    
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="30"></ColumnDefinition>
                                </Grid.ColumnDefinitions>

                                <Border Grid.Row="0" Grid.ColumnSpan="3" Style="{DynamicResource PurpleBorder}">
                                    <TextBlock Style="{DynamicResource PurpleBorderInfo}" Text="Photo"></TextBlock>
                                </Border>

                                <Grid Grid.Row="2" Grid.ColumnSpan="2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="5"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>

                                    <Border Height="128" Width="140" Style="{DynamicResource GroupBorder}" BorderThickness="1" >
                                        <Image x:Name="pictPhoto" Stretch="Uniform"/>
                                    </Border>
                                    <Grid Grid.Column="2" Width="120" HorizontalAlignment="Right">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="9"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="9"/>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>
                                        <Button Grid.Row="0" Width="120" 
                                            Style="{DynamicResource DeletePhotoButton}" x:Name="btnClearImage" Click="BtnClearImage_Click" >

                                        </Button>
                                        <Button Grid.Row="2" Width="120"  
                                Style="{DynamicResource ScanPhotoButton}" x:Name="btnScanImage">

                                        </Button>
                                        <Button Grid.Row="4" Width="120" MinHeight="30" 
                                Style="{DynamicResource SelectPhotoButton}" x:Name="button1" Click="Button1_Click" >

                                        </Button>
                                    </Grid>
                                </Grid>


                                <Border Grid.Row="4" Grid.ColumnSpan="2" Style="{DynamicResource OrangeBorder}">
                                    <TextBlock Style="{DynamicResource OrangeBorderInfo}" Text="Visibility &amp; others"></TextBlock>
                                </Border>

                                <dxe:ToggleSwitch ContentPlacement="Far" HorizontalAlignment="Left" Grid.Row="6" UncheckedStateContent="Active ?" CheckedStateContent="Active ?"  IsChecked="True" x:Name="chkActive" Checked="ChkActive_Checked" Unchecked="ChkActive_Checked"/>
                                <dxe:ToggleSwitch ContentPlacement="Far" HorizontalAlignment="Left" Grid.Row="8" UncheckedStateContent="Add to POS Main Screen" CheckedStateContent="Add to POS Main Screen"  x:Name="chkPOSScreen" Checked="ChkActive_Checked" Unchecked="ChkActive_Checked"/>
                                <dxe:ToggleSwitch ContentPlacement="Far" HorizontalAlignment="Left" Grid.Row="10" UncheckedStateContent="Add to POS Category Screen" CheckedStateContent="Add to POS Category Screen"  x:Name="chkPOSCatScreen" Checked="ChkActive_Checked" Unchecked="ChkActive_Checked"/>
                                <dxe:ToggleSwitch ContentPlacement="Far" HorizontalAlignment="Left" Grid.Row="12" UncheckedStateContent="Prompt for Price"  CheckedStateContent="Prompt for Price" x:Name="chkPromptPrice" Checked="ChkActive_Checked" Unchecked="ChkActive_Checked"/>
                                
                            </Grid>
                        </Grid>







                    </TabItem>
                    <TabItem  Width="{Binding ElementName=c1, Path=ActualWidth}" x:Name="tpStyle"
                         Style="{DynamicResource OuterPOSTabItemStyle}" PreviewMouseLeftButtonUp="TpStyle_PreviewMouseLeftButtonUp">
                        <TabItem.Header>
                            <TextBlock Text="Notes &amp; Style" Style="{DynamicResource TabItemText}"></TextBlock>
                        </TabItem.Header>


                        <Grid Margin="9">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="2"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="2"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>

                            <Grid Grid.Column="0" Margin="7,0,7,0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="9"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
  
                                </Grid.RowDefinitions>
                                

                                <Border Grid.Row="0" Grid.ColumnSpan="3" Style="{DynamicResource PurpleBorder}">
                                    <TextBlock Style="{DynamicResource PurpleBorderInfo}" Text="Notes"></TextBlock>
                                </Border>

                                <dxe:TextEdit Grid.Row="2" Height="150" x:Name="txtNotes"
                                              VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled"
                                              VerticalContentAlignment="Top" AcceptsReturn="True" TextWrapping="Wrap" EditValueChanged="TxtSKU_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus"/>

                            </Grid>
                            <Grid Grid.Column="1" Style="{DynamicResource ScreenSeparator}"></Grid>
                            <Grid Grid.Column="2" Margin="7,0,7,0">

                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                </Grid.RowDefinitions>
                                

