    <!--Width="1024" Height="768"-->
    <UserControl.Resources>
        <ResourceDictionary>
            <dxre:HtmlToContentConverter x:Key="htmlToContentConverter"></dxre:HtmlToContentConverter>
            <dxre:RtfToContentConverter x:Key="rtfToContentConverter" />
            <Style x:Key="richEditStyle" TargetType="dxre:RichEditControl">

                <Setter Property="ShowBorder" Value="false"/>
                <Setter Property="ActiveViewType" Value="Simple"/>
                <Setter Property="VerticalScrollBarVisibility" Value="Collapsed"/>
                <Setter Property="HorizontalScrollBarVisibility" Value="Collapsed"/>
            </Style>

            <Style TargetType="Border" x:Key="borderStyle">
                <Setter Property="Visibility" Value="Collapsed"/>
                <Style.Triggers>
                    <Trigger Property="dxg:GridViewBase.IsFocusedRow" Value="True">
                        <Setter Property="Visibility" Value="Visible"></Setter>
                    </Trigger>
                </Style.Triggers>
            </Style>
            <Style TargetType="{x:Type dxg:NoRecordsControl}" x:Key="{dxgt:TableViewThemeKey ResourceKey=NoRecordsControlStyle, IsThemeIndependent=true}">
                <!--!!!-->
                <Style.Resources>
                    <dxg:DynamicLocalizationStringValueConvertor x:Key="LocalizationStringValueConverter" />
                </Style.Resources>
                <Setter Property="NoRecordsFoundText" Value="{Binding Path=DataControl.View.LocalizationDescriptor, Converter={StaticResource LocalizationStringValueConverter}, ConverterParameter=NoRecordsFound, RelativeSource={RelativeSource Self}}" />
                <Setter Property="NoRecordsText"  Value="{Binding Path=DataControl.View.LocalizationDescriptor, Converter={StaticResource LocalizationStringValueConverter}, ConverterParameter=NoRecords, RelativeSource={RelativeSource Self}}" />
                <!--!!!-->

                <Setter Property="DataControl" Value="{Binding Path=(dxg:GridControl.CurrentView).DataControl, RelativeSource={RelativeSource TemplatedParent}}" />
                <Setter Property="Foreground" Value="{DynamicResource {dxgt:TableViewThemeKey ResourceKey=NoRecordsForeground}}" />
                <Setter Property="HorizontalAlignment" Value="Center" />
                <Setter Property="HorizontalContentAlignment" Value="Center" />
                <Setter Property="Opacity" Value="1.00" />
                <Setter Property="OverridesDefaultStyle" Value="True" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type dxg:NoRecordsControl}">
                            <StackPanel Orientation="Horizontal">
                                <TextBlock x:Name="noRecordsTxt"
                                           Text="{TemplateBinding NoRecordsFoundText}" />
                                <Button Margin="3,0"
                                        Command="{Binding DataContext.MyCommand,
                                                          ElementName=MyWindowInstance}"
                                        Content="TEST BUTTON" />
                            </StackPanel>
                            <ControlTemplate.Triggers>
                                <DataTrigger Binding="{Binding NoRecords,
                                                               RelativeSource={RelativeSource Self}}"
                                             Value="True">
                                    <Setter TargetName="noRecordsTxt" Property="Text" Value="{Binding NoRecordsText, RelativeSource={RelativeSource TemplatedParent}}" />
                                </DataTrigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
                <Setter Property="VerticalAlignment" Value="Top" />
                <Setter Property="VerticalContentAlignment" Value="Center" />
            </Style>


            <!--<Style TargetType="dxg:GridControl">
                <Setter Property="Background" Value="Black" />
            </Style>-->

            <!--<Style  TargetType="{x:Type dxg:RowControl}">
                <Style.Triggers>
                    <DataTrigger Binding="{Binding SelectionState}" Value="None">
                        <Setter Property="Background" Value="White"/>
                    </DataTrigger>
                </Style.Triggers>
            </Style>-->

            <ControlTemplate x:Key="{dxgt:TableViewThemeKey ResourceKey=DataPresenterTemplate}" TargetType="{x:Type dxg:DataPresenter}">
                <Border Background="Red">
                    <ContentPresenter />
                </Border>
            </ControlTemplate>
            <Style x:Key="RowStyle" TargetType="{x:Type dxg:GridRowContent}">
                <Setter Property="Background" Value="Red" />
            </Style>

            <!--<ControlTemplate xmlns:dxgt="http://schemas.devexpress.com/winfx/2008/xaml/grid/themekeys"  
                 xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"  
                 x:Key="{dxgt:GridControlThemeKey ResourceKey=BorderTemplate, IsThemeIndependent=True}"  
                 TargetType="{x:Type ContentControl}">
                <Border Background="White">
                    <Border.Padding>
                        <Binding Path="(dxg:GridBorderHelper.ShowBorder)" RelativeSource="{RelativeSource TemplatedParent}">
                            <Binding.Converter>
                                <dx:BoolToObjectConverter FalseValue="0" TrueValue="1" />
                            </Binding.Converter>
                        </Binding>
                    </Border.Padding>
                    <Border  Style="{DynamicResource WhiteBackcolor}"  >
                        <ContentPresenter />
                    </Border>
                </Border>
            </ControlTemplate>-->

        </ResourceDictionary>

