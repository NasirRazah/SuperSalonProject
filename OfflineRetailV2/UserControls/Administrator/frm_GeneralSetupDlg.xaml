<Window x:Class="OfflineRetailV2.UserControls.Administrator.frm_GeneralSetupDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OfflineRetailV2.UserControls.Administrator"
           xmlns:cc="clr-namespace:OfflineRetailV2.Controls"
        xmlns:lm="clr-namespace:OfflineRetailV2.LayoutManager"
        xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
        xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid" 
        xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
        xmlns:dxcn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal" 
         xmlns:Custom="http://schemas.devexpress.com/winfx/2008/xaml/charts/rangecontrolclient" 
        AllowsTransparency="True" WindowStyle="None" Background="Transparent" WindowStartupLocation="CenterScreen" Margin="50"
        Title="" Foreground="White"   
        Width="1024" Height="740"  Closing="Window_Closing">
    <Window.Resources>
        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
    </Window.Resources>
    <cc:ModalWindow  x:Name="ModalWindow" Margin="0,25,0,0">
        <Grid Margin="9">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="3"/>
                <RowDefinition Height="*"/>

            </Grid.RowDefinitions>
            <Grid Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <TextBlock Text="General Settings"  x:Name="Title" VerticalAlignment="Center" Style="{DynamicResource BlueHeading}" />
                <Grid Grid.Column="1" HorizontalAlignment="Right" Margin="0,0,20,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                        <ColumnDefinition Width="9"></ColumnDefinition>
                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                        <ColumnDefinition Width="9"></ColumnDefinition>
                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <dxe:ToggleSwitch ContentPlacement="Far" HorizontalAlignment="Left" CheckedStateContent="Use 4 Digit Decimal in Purchase &amp; Receiving" UncheckedStateContent="Use 4 Digit Decimal in Purchase &amp; Receiving" FontSize="13" x:Name="chk4digit" Visibility="Hidden"/>
                    <!--<TextBlock Grid.Column="0" x:Name="txtDBVersion" FontWeight="SemiBold" TextWrapping="Wrap" Style="{DynamicResource GreenBorderInfo}" Text="DB Version" Visibility="Collapsed"></TextBlock>-->
                    <Button Grid.Column="2" Content="CANCEL" Height="35" Width="120" FontFamily="{StaticResource OSSemiBold}" 
                        Style="{DynamicResource CancelButtonStyle}" x:Name="btnCancel" Click="Button_Click_1" />
                    <Button Grid.Column="4" Content="SAVE" Height="35" Width="120" FontFamily="{StaticResource OSSemiBold}" 
                        Style="{DynamicResource SaveButtonStyle}" x:Name="btnLogin" Click="Button_Click"/>
                </Grid>
            </Grid>
            <Grid Grid.Row="2" Margin="0,9,0,0">
                <dx:DXTabControl Grid.Row="2" x:Name="tcSetup" Style="{DynamicResource DxTabControl}" HorizontalContentAlignment="Stretch"  SelectionChanged="TcSetup_SelectionChanged">
                    <dx:DXTabControl.View>
                        <dx:TabControlScrollView AllowHideTabItems="False"/>
                    </dx:DXTabControl.View>
                    <dx:DXTabItem  x:Name="tpOffice" PreviewMouseLeftButtonUp="TpOffice_PreviewMouseLeftButtonUp"  Height="40" Width="175" >

                        <dx:DXTabItem.HeaderTemplate>
                            <DataTemplate>
                                <TextBlock  Text="      Store Info  " FontFamily="{DynamicResource OSSemiBold}" Style="{DynamicResource TabItemTextD}" Height="40" HorizontalAlignment="Stretch" TextAlignment="Center" Margin="0,-9,0,0"  />
                            </DataTemplate>
                        </dx:DXTabItem.HeaderTemplate>
                        <ScrollViewer Margin="0" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled" PanningMode="Both" PanningRatio="1" PanningDeceleration="2" ManipulationBoundaryFeedback="ScrlViewer_ManipulationBoundaryFeedback">
                            <Grid Margin="5">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="1"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="30"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="15"/>
                                        <RowDefinition Height="Auto"/>
                                    </Grid.RowDefinitions>
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="9"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <Border Style="{DynamicResource GreenBorder}">
                                            <TextBlock Style="{DynamicResource GreenBorderInfo}" Text="Company Information"></TextBlock>
                                        </Border>
                                        <Grid Grid.Row="2">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="9"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto"></RowDefinition>
                                                <RowDefinition Height="3"></RowDefinition>
                                                <RowDefinition Height="Auto"></RowDefinition>
                                                <RowDefinition Height="3"></RowDefinition>
                                                <RowDefinition Height="Auto"></RowDefinition>
                                                <RowDefinition Height="3"></RowDefinition>
                                                <RowDefinition Height="Auto"></RowDefinition>
                                                <RowDefinition Height="3"></RowDefinition>
                                                <RowDefinition Height="Auto"></RowDefinition>
                                                <RowDefinition Height="3"></RowDefinition>
                                                <RowDefinition Height="Auto"></RowDefinition>
                                                <RowDefinition Height="3"></RowDefinition>
                                                <RowDefinition Height="Auto"></RowDefinition>
                                                <RowDefinition Height="3"></RowDefinition>
                                                <RowDefinition Height="Auto"></RowDefinition>
                                                <RowDefinition Height="3"></RowDefinition>
                                                <RowDefinition Height="Auto"></RowDefinition>
                                                <RowDefinition Height="3"></RowDefinition>
                                                <RowDefinition Height="Auto"></RowDefinition>
                                                <RowDefinition Height="3"></RowDefinition>
                                                <RowDefinition Height="Auto"></RowDefinition>
                                                <RowDefinition Height="3"></RowDefinition>
                                                <RowDefinition Height="Auto"></RowDefinition>
                                                <RowDefinition Height="3"></RowDefinition>
                                            </Grid.RowDefinitions>

                                            <TextBlock Grid.Row="0" Grid.Column="0" Text="Company"/>
                                            <dxe:TextEdit Grid.Row="2" Grid.Column="0" x:Name="txtCompany" IsReadOnly="True" />
                                            <TextBlock Grid.Row="4" Grid.Column="0" Text="Address 1"/>
                                            <dxe:TextEdit Grid.Row="6" Grid.Column="0" x:Name="txtAdd1" IsReadOnly="True" />
                                            <TextBlock Grid.Row="8" Grid.Column="0" Text="Address 2"/>
                                            <dxe:TextEdit Grid.Row="10" Grid.Column="0" x:Name="txtAdd2" IsReadOnly="True" />
                                            <Grid Grid.Row="12" Grid.Column="0">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="1.5*"/>
                                                    <ColumnDefinition Width="9"/>
                                                    <ColumnDefinition Width="*"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="3"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                </Grid.RowDefinitions>
                                                <TextBlock Grid.Row="0" Grid.Column="0" Text="City"/>
                                                <dxe:TextEdit Grid.Row="2" Grid.Column="0" x:Name="txtCity" IsReadOnly="True" />
                                                <TextBlock Grid.Row="0" Grid.Column="2" Text="Zip"/>
                                                <dxe:TextEdit Grid.Row="2" Grid.Column="2" x:Name="cmbZip" IsReadOnly="True"/>
                                            </Grid>
                                            <TextBlock  Grid.Row="14" Grid.Column="0" Text="State"/>
                                            <dxe:TextEdit Grid.Row="16" Grid.Column="0" x:Name="txtState" IsReadOnly="True"/>

                                            <TextBlock Grid.Row="0" Grid.Column="2" Text="Phone" />
                                            <dxe:TextEdit Grid.Row="2" Grid.Column="2" x:Name="txtPhone" MaxLength="14" MaskType="Simple" MaskUseAsDisplayFormat="True" EditValueChanged="TxtPhone_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus"/>
                                            <TextBlock Grid.Row="4" Grid.Column="2" Text="Fax"/>
                                            <dxe:TextEdit Grid.Row="6" Grid.Column="2" x:Name="txtFax" MaxLength="14" EditValueChanged="TxtPhone_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus"/>
                                            <TextBlock Grid.Row="8" Grid.Column="2" Text="Email "/>
                                            <dxe:TextEdit Grid.Row="10" Grid.Column="2" x:Name="txtEmail" IsReadOnly="True"/>

                                            <Grid Grid.Row="12" Grid.Column="2">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                    <RowDefinition Height="3"></RowDefinition>
                                                    <RowDefinition Height="Auto"></RowDefinition>
                                                </Grid.RowDefinitions>
                                                <TextBlock Grid.Row="0"  Text="Web Address"/>
                                                <dxe:TextEdit Grid.Row="2"  x:Name="txtWebAddress" MaxLength="30" EditValueChanged="TxtPhone_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus" />

                                            </Grid>

                                            <TextBlock Grid.Row="14" Grid.Column="2" Text="Office Informations" />
                                            <dxe:TextEdit Grid.Row="16"  Grid.Column="2" MaxLength="100" x:Name="txtMsg" HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Auto" VerticalContentAlignment="Top" AcceptsReturn="True" TextWrapping="Wrap" EditValueChanged="TxtPhone_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus"/>

                                        </Grid>
                                    </Grid>

                                    <Grid Grid.Row="2">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"></RowDefinition>
                                            <RowDefinition Height="3"></RowDefinition>
                                            <RowDefinition Height="Auto"></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <Border Style="{DynamicResource OrangeBorder}">
                                            <TextBlock Text="Logo  / Webpage" Style="{DynamicResource OrangeBorderInfo}"></TextBlock>
                                        </Border>

                                        <Grid Grid.Row="2">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="11"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto"/>
                                                    <RowDefinition Height="11"/>
                                                    <RowDefinition Height="Auto"/>
                                                    <RowDefinition Height="11"/>
                                                    <RowDefinition Height="Auto"/>
                                                </Grid.RowDefinitions>
                                                <RadioButton Content="Logo" GroupName="1" x:Name="rglogo_0" Checked="Rglogo_0_Checked" Unchecked="Rglogo_0_Checked" FontSize="14" />
                                                <RadioButton Grid.Row="2" Content="Webpage" GroupName="1" x:Name="rglogo_1" FontSize="14" Checked="Rglogo_0_Checked" Unchecked="Rglogo_0_Checked"/>
                                                    <dxe:ToggleSwitch Grid.Row="4"  x:Name="chkPrintLogo" ContentPlacement="Far" CheckedStateContent="Print logo in receipt" UncheckedStateContent="Print logo in receipt" HorizontalAlignment="Left" IsChecked="True" Checked="ChkPrintLogo_Checked" Unchecked="ChkPrintLogo_Checked" ></dxe:ToggleSwitch>


                                                </Grid>
                                            <Grid Grid.Column="2" >
                                                <TabControl x:Name="tabLogo" Style="{DynamicResource NoTab}" >
                                                    <TabControl.ItemContainerStyle>
                                                        <Style TargetType="{x:Type TabItem}">
                                                            <Setter Property="Visibility" Value="Collapsed"/>
                                                        </Style>
                                                    </TabControl.ItemContainerStyle>
                                                    <TabItem Header="Logo" x:Name="tplogo" >
                                                        <Grid Grid.Column="2">
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="*"/>
                                                                <ColumnDefinition Width="5"/>
                                                                <ColumnDefinition Width="*"/>
                                                            </Grid.ColumnDefinitions>

                                                            <Border Height="128" Width="140" Style="{DynamicResource GroupBorder}" BorderThickness="1" >
                                                                <Image x:Name="pictPhoto" Stretch="Uniform"/>
                                                            </Border>
                                                            <Grid Grid.Column="2" Width="120" HorizontalAlignment="Right">
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition Height="*"/>
                                                                    <RowDefinition Height="9"/>
                                                                    <RowDefinition Height="*"/>
                                                                    <RowDefinition Height="9"/>
                                                                    <RowDefinition Height="*"/>
                                                                </Grid.RowDefinitions>
                                                                <Button Grid.Row="0" Width="120" 
                                            Style="{DynamicResource DeletePhotoButton}" x:Name="btnClearImage" Click="BtnClearImage_Click" >

                                                                </Button>
                                                                <Button Grid.Row="2" Width="120"  
                                Style="{DynamicResource ScanPhotoButton}" x:Name="btnScanImage">

                                                                </Button>
                                                                <Button Grid.Row="4" Width="120" MinHeight="30" 
                                Style="{DynamicResource SelectPhotoButton}" x:Name="btnOpen" Click="BtnOpen_Click" >

                                                                </Button>
                                                            </Grid>
                                                        </Grid>
                                                    </TabItem>
                                                    <TabItem Header="Web" x:Name="tpfixedweb">
                                                        <Grid Margin="6">
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="Auto"/>
                                                                <RowDefinition Height="9"/>
                                                                <RowDefinition Height="Auto"/>
                                                            </Grid.RowDefinitions>
                                                            <Grid Grid.Row="0">
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="Auto"/>
                                                                    <ColumnDefinition Width="13"/>
                                                                    <ColumnDefinition Width="*"/>

                                                                </Grid.ColumnDefinitions>
                                                                <TextBlock Text="Webpage"/>
                                                                <dxe:TextEdit Grid.Column="2" x:Name="txtFixedWeb" MaxLength="256" EditValueChanged="TxtPhone_EditValueChanged"/>
                                                            </Grid>
                                                            <Grid Grid.Row="2">
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition Height="Auto"/>
                                                                    <RowDefinition Height="13"/>
                                                                    <RowDefinition Height="Auto"/>
                                                                    <RowDefinition Height="13"/>
                                                                    <RowDefinition Height="Auto"/>
                                                                </Grid.RowDefinitions>
                                                                <RadioButton Grid.Row="0" Grid.Column="0" Content="Allow visiting a Fixed Webpage" GroupName="2" x:Name="rgwebsec_0" IsChecked="True"/>
                                                                <RadioButton Grid.Row="2" Grid.Column="0" Content="Allow visiting any Webpage" GroupName="2" x:Name="rgwebsec_1"/>
                                                                <RadioButton Grid.Row="4" Grid.Column="0" Content="Do not allow visiting a Webpage" GroupName="2" x:Name="rgwebsec_2"/>
                                                            </Grid>
                                                        </Grid>
                                                    </TabItem>
                                                </TabControl>
                                            </Grid>

                                        </Grid>

                                    </Grid>

                                </Grid>
                                <Grid Grid.Column="2">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="9"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="11"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="11"/>
                                        <RowDefinition Height="Auto"/>
                                    </Grid.RowDefinitions>


                                    <Border Style="{DynamicResource PurpleBorder}">
                                        <TextBlock Text="SMTP Configuration for sending Reports" Style="{DynamicResource PurpleBorderInfo}"></TextBlock>
                                    </Border>


                                    <Grid Grid.Row="2">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>


                                        </Grid.RowDefinitions>

                                        <TabControl Grid.Row="2" Margin="-1" BorderThickness="0" x:Name="xtraTabControl1"
                                                    Style="{DynamicResource InnerPOSTabControlStyle}">
                                            <TabItem  x:Name="xtraTabPage1"
                                                     Style="{DynamicResource InnerPOSTabItemStyle}">
                                                <TabItem.Header>
                                                    <TextBlock Text="SMTP Setup" Style="{DynamicResource TabItemTextI}"></TextBlock>
                                                </TabItem.Header>

                                                <Grid Margin="9">
                                                    <Grid.RowDefinitions>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="3"/>
                                                            <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="3"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="3"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="3"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="3"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="3"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="3"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="3"/>
                                                        <RowDefinition Height="Auto"/>
                                                    </Grid.RowDefinitions>

                                                        <dxe:ComboBoxEdit  Grid.Row="2" SelectedIndex="0" x:Name="cmbSMTPType" IsTextEditable="False"  AutoComplete="True" ImmediatePopup="True" PreviewMouseLeftButtonDown="CmbLanguage_PreviewMouseLeftButtonDown" SelectedIndexChanged="CmbSMTPType_SelectedIndexChanged">
                                                            <dxe:ComboBoxEditItem>Custom</dxe:ComboBoxEditItem>
                                                            <dxe:ComboBoxEditItem>Gmail</dxe:ComboBoxEditItem>
                                                        </dxe:ComboBoxEdit>

                                                        <TextBlock Grid.Row="4" Text="Host"/>
                                                        <dxe:TextEdit Grid.Row="6" Grid.Column="2" x:Name="txtREHost" MaxLength="100" EditValueChanged="TxtREHost_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus" PreviewMouseLeftButtonDown="Full_PreviewMouseLeftButtonDown"/>
                                                    <TextBlock Grid.Row="8" Text="User Name"/>
                                                        <dxe:TextEdit Grid.Row="10" Grid.Column="2" x:Name="txtREUser" MaxLength="100" EditValueChanged="TxtPhone_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus" PreviewMouseLeftButtonDown="Full_PreviewMouseLeftButtonDown"/>
                                                    <TextBlock Grid.Row="12" Text="Password"/>
                                                        <dxe:PasswordBoxEdit  MaxLength="100"  Height="32" Grid.Row="14" Grid.Column="2" x:Name="txtREPassword" EditValueChanged="TxtPhone_EditValueChanged" GotFocus="Full_GotFocusP" LostFocus="Full_LostFocus" />
                                                    <TextBlock Grid.Row="16" Text="Port"/>
                                                    <Grid Grid.Row="18" >
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="93*"/>
                                                            <ColumnDefinition Width="117*"/>
                                                            <ColumnDefinition Width="35*"/>
                                                            <ColumnDefinition Width="174*"/>
                                                        </Grid.ColumnDefinitions>
                                                            <dxe:SpinEdit Grid.Column="0" Text="0"  HorizontalContentAlignment="Center" x:Name="txtREPort" EditValueChanged="TxtPhone_EditValueChanged" Grid.ColumnSpan="2">
                                                                <dxe:SpinEdit.Buttons>
                                                                    <dxe:ButtonInfoCollection>
                                                                        <dxe:ButtonInfo Command="{Binding SpinDownCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=dxe:SpinEdit}}">
                                                                            <dxe:ButtonInfo.Content>
                                                                                <dx:DXImage Source="{DynamicResource GreenDropdownD}" Height="15" Width="16"  Margin="5,5">
                                                                                </dx:DXImage>
                                                                            </dxe:ButtonInfo.Content>
                                                                        </dxe:ButtonInfo>
                                                                        <dxe:ButtonInfo Command="{Binding SpinUpCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=dxe:SpinEdit}}" IsLeft="True">
                                                                            <dxe:ButtonInfo.Content>
                                                                                <dx:DXImage Source="{DynamicResource GreenDropdownU}" Height="15" Width="16"  Margin="5,5">
                                                                                </dx:DXImage>
                                                                            </dxe:ButtonInfo.Content>
                                                                        </dxe:ButtonInfo>
                                                                    </dxe:ButtonInfoCollection>
                                                                </dxe:SpinEdit.Buttons>
                                                            </dxe:SpinEdit>
                                                        <dxe:ToggleSwitch Grid.Column="3"  x:Name="chkRESSL" ContentPlacement="Far" CheckedStateContent="Enable SSL?" UncheckedStateContent="Enable SSL?" HorizontalAlignment="Left" Checked="ChkSecondMonitor_Checked" Unchecked="ChkSecondMonitor_Checked"></dxe:ToggleSwitch>

                                                    </Grid>
                                                </Grid>
                                            </TabItem>
                                            <TabItem 
                                                     Style="{DynamicResource InnerPOSTabItemStyle}">
                                                <TabItem.Header>
                                                    <TextBlock Text="Email Address Setup" Style="{DynamicResource TabItemTextI}"></TextBlock>
                                                </TabItem.Header>
                                                <Grid Margin="9">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="3"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="3"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="3"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="3"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="3"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="3"/>
                                                        <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="3"/>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="3"/>
                                                            <RowDefinition Height="Auto"/>
                                                        </Grid.RowDefinitions>

                                                    <TextBlock Grid.Row="0" Text="To"/>
                                                        <dxe:TextEdit Grid.Row="2" Grid.Column="2" x:Name="txtReportEmail" MaxLength="500" EditValueChanged="TxtPhone_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus" PreviewMouseLeftButtonDown="Full_PreviewMouseLeftButtonDown"/>
                                                    <TextBlock Grid.Row="4" Grid.Column="2" HorizontalAlignment="Right"
                                                               Text="* Enter multiple emails separated by a comma"
                                                               VerticalAlignment="Top" Margin="6,3,0,0"/>
                                                    <TextBlock Grid.Row="6" Text="From Address"/>
                                                        <dxe:TextEdit Grid.Row="8" Grid.Column="2" x:Name="txtREFromEmail" MaxLength="100" EditValueChanged="TxtPhone_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus" PreviewMouseLeftButtonDown="Full_PreviewMouseLeftButtonDown"/>
                                                    <TextBlock Grid.Row="10" Text="From Name"/>
                                                        <dxe:TextEdit Grid.Row="12" Grid.Column="2" x:Name="txtREFromName" MaxLength="100" EditValueChanged="TxtPhone_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus" PreviewMouseLeftButtonDown="Full_PreviewMouseLeftButtonDown"/>
                                                    <TextBlock Grid.Row="14" Text="Reply To"/>
                                                        <dxe:TextEdit Grid.Row="16" Grid.Column="2" x:Name="txtREReply" MaxLength="100" EditValueChanged="TxtPhone_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus" PreviewMouseLeftButtonDown="Full_PreviewMouseLeftButtonDown"/>
                                                </Grid>
                                            </TabItem>
                                        </TabControl>
                                        <Border Grid.Row="3" Margin="0,0,0,10" Style="{DynamicResource DummyBorder}">
                                            <Button  Content="Test Email Setup" Height="35" x:Name="btnTestEmail"
                                                Style="{DynamicResource GeneralButtonStyle}" Margin="33,0" Click="BtnTestEmail_Click"/>
                                        </Border>

                                    </Grid>
                                    <Grid Grid.Row="3">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"></RowDefinition>

                                            <RowDefinition Height="Auto"></RowDefinition>
                                            <RowDefinition Height="Auto"></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <Border Style="{DynamicResource BlueBorder}">
                                            <TextBlock Text="Second Monitor" Style="{DynamicResource PurpleBorderInfo}"></TextBlock>
                                        </Border>
                                        <Border Grid.Row="1" Style="{DynamicResource DummyBorder}">
                                            <Grid Grid.Row="4" Margin="10">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto"/>
                                                    <RowDefinition Height="3"/>
                                                    <RowDefinition Height="Auto"/>
                                                    <RowDefinition Height="3"/>
                                                    <RowDefinition Height="Auto"/>
                                                    <RowDefinition Height="3"/>
                                                    <RowDefinition Height="Auto"/>
                                                    <RowDefinition Height="9"/>
                                                    <RowDefinition Height="Auto"/>
                                                </Grid.RowDefinitions>
                                                <dxe:ToggleSwitch Grid.Row="2"  x:Name="chkSecondMonitor" ContentPlacement="Far" CheckedStateContent="Activate Second Monitor" UncheckedStateContent="Activate Second Monitor" HorizontalAlignment="Left" Checked="ChkSecondMonitor_Checked" Unchecked="ChkSecondMonitor_Checked"></dxe:ToggleSwitch>
                                                <dxe:ToggleSwitch Grid.Row="4"  x:Name="chkSMWithPOS" ContentPlacement="Far" CheckedStateContent="Use Customer Display When POS Opens" UncheckedStateContent="Use Customer Display When POS Opens" HorizontalAlignment="Left" Checked="ChkSecondMonitor_Checked" Unchecked="ChkSecondMonitor_Checked" ></dxe:ToggleSwitch>
                                                <Grid Grid.Row="6">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="Auto"/>
                                                        <ColumnDefinition Width="13"/>
                                                        <ColumnDefinition Width="*"/>
                                                        <ColumnDefinition Width="13"/>
                                                        <ColumnDefinition Width="Auto"/>
                                                    </Grid.ColumnDefinitions>
                                                    <TextBlock Text="Application"/>
                                                    <dxe:TextEdit Grid.Column="2" x:Name="txtSecondMonitorApp" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus"/>
                                                    <Button Grid.Column="4" Content="BROWSE" Height="32" Width="100" x:Name="label47"
                                                    Style="{DynamicResource BrowseButtonStyle}" Click="Label47_Click"/>
                                                </Grid>
                                                <Button Grid.Row="8" Content="CUSTOMER DISPLAY SETUP" Height="35" x:Name="label52"
                                                    Style="{DynamicResource GeneralButtonStyle}" Click="Label52_Click"/>
                                            </Grid>
                                        </Border>

                                        <Grid Grid.Row="2">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="11"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="Language" x:Name="lbLanguage"/>
                                            <dxe:ComboBoxEdit Height="25" Grid.Column="2" x:Name="cmbLanguage" SelectedIndex="0" IsTextEditable="False" PreviewMouseLeftButtonDown="CmbLanguage_PreviewMouseLeftButtonDown">
                                                <dxe:ComboBoxEditItem>English</dxe:ComboBoxEditItem>
                                                <dxe:ComboBoxEditItem>Spanish</dxe:ComboBoxEditItem>
                                            </dxe:ComboBoxEdit>
                                        </Grid>
