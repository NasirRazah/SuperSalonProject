                            <TextBlock Grid.Row="2" Grid.Column="0" Text="Store Code"></TextBlock>
                        <dxe:TextEdit Grid.Row="2" Grid.Column="2" x:Name="txtStoreCode" MaxLength="10" EditValueChanged="TxtStoreCode_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus"></dxe:TextEdit>
                            <TextBlock Grid.Row="4" Grid.Column="0" Text="Store Name"></TextBlock>
