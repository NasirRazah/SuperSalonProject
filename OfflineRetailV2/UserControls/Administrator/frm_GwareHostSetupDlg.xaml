<Window x:Class="OfflineRetailV2.UserControls.Administrator.frm_GwareHostSetupDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OfflineRetailV2.UserControls.Administrator"
            xmlns:cc="clr-namespace:OfflineRetailV2.Controls"
        xmlns:lm="clr-namespace:OfflineRetailV2.LayoutManager"
        xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
        xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid" 
        xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
        AllowsTransparency="True" WindowStyle="None" Background="Transparent" WindowStartupLocation="CenterScreen"
        Title="" Foreground="White"   Height="740" Width="1024" Loaded="Window_Loaded" Closing="Window_Closing" >
    <cc:ModalWindow  x:Name="ModalWindow" >

        <Grid Margin="9">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="9"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="9"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <Grid Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <TextBlock Text="Title"  x:Name="Title" VerticalAlignment="Center" Style="{DynamicResource BlueHeading}" />
                <Grid Grid.Column="1" HorizontalAlignment="Right" Margin="0,0,20,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                        <ColumnDefinition Width="9"></ColumnDefinition>
                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                    </Grid.ColumnDefinitions>

                    <Button Grid.Column="0" Content="CANCEL" Height="35" Width="120" FontFamily="{StaticResource OSSemiBold}" 
                        Style="{DynamicResource CancelButtonStyle}" x:Name="btnCancel" Click="BtnCancel_Click" />
                    <Button Grid.Column="2" Content="SAVE" Height="35" Width="120" FontFamily="{StaticResource OSSemiBold}" 
                        Style="{DynamicResource SaveButtonStyle}" x:Name="btnOK" Click="BtnOK_Click"/>
                </Grid>
            </Grid>
            <Rectangle Grid.Row="1" Style="{DynamicResource DefaultLineStyle}"  VerticalAlignment="Center"  Margin="-9,0"/>

            <Grid Grid.Row="2" Margin="0,9,0,0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="35"></RowDefinition>
                    <RowDefinition Height="*"></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                    <ColumnDefinition Width="21"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                </Grid.ColumnDefinitions>

                <dxe:ToggleSwitch ContentPlacement="Far" HorizontalAlignment="Left" Grid.Row="0" CheckedStateContent="Enable XEPOS Host Operations" UncheckedStateContent="Enable XEPOS Host Operations" x:Name="chkCentral" Checked="ChkCentral_Checked" Unchecked="ChkCentral_Checked"/>

                <Grid Grid.Column="0" Grid.Row="1">

                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="9"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="35"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="9"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="20"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="9"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="2"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                    </Grid.RowDefinitions>

                    <Border Grid.Row="0" Style="{DynamicResource GreenBorder}">
                        <TextBlock Style="{DynamicResource GreenBorderInfo}" Text="Store Setup"/>
                    </Border>

                    <Grid  Grid.Row="2" x:Name="gcco1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="9"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="3"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="3"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="3"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                        </Grid.RowDefinitions>

                        <TextBlock Grid.Row="0" Grid.ColumnSpan="3" Text="Store Code must be unique for all stores under XEPOS Host."></TextBlock>
                        <TextBlock Grid.Row="2" Grid.Column="0" Text="Store Code"></TextBlock>
                        <dxe:TextEdit Grid.Row="4" Grid.Column="2" x:Name="txtStoreCode" MaxLength="10" EditValueChanged="TxtStoreCode_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus"></dxe:TextEdit>
                        <TextBlock Grid.Row="6" Grid.Column="0" Text="Store Name"></TextBlock>
                        <dxe:TextEdit Grid.Row="8" Grid.Column="2" x:Name="txtStoreName" MaxLength="50" EditValueChanged="TxtStoreCode_EditValueChanged" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus"></dxe:TextEdit>
                    </Grid>

                    <Border Grid.Row="4" Style="{DynamicResource OrangeBorder}">
                        <TextBlock Style="{DynamicResource OrangeBorderInfo}" Text="Connect to XEPOS Account"/>
                    </Border>

                    <Grid Grid.Row="6" x:Name="gcco3">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="3"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="3"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="3"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="9"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                        </Grid.RowDefinitions>

                        <TextBlock Grid.Row="0" Grid.Column="0" Text="Customer Code"></TextBlock>
                        <dxe:TextEdit Grid.Row="2" Grid.Column="2" x:Name="txtClientCode" MaxLength="20" GotFocus="Full_GotFocus" LostFocus="Full_LostFocus"></dxe:TextEdit>
                        <TextBlock Grid.Row="4" Grid.Column="0" Text="Password"></TextBlock>
                        <dxe:PasswordBoxEdit Grid.Row="6" Grid.Column="2"  Height="32" x:Name="txtClientPassword" MaxLength="20" GotFocus="Full_GotFocusPswd" LostFocus="Full_LostFocus"></dxe:PasswordBoxEdit>
                        <Grid Grid.Row="8" >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="9"></ColumnDefinition>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Button Grid.Column="0" x:Name="btnClientLogout" Style="{DynamicResource GeneralButtonStyle}" Content="LOGOUT" Height="32" Click="BtnClientLogout_Click" >
                            </Button>
                            <Button Grid.Column="2" x:Name="btnClientLogin" Style="{DynamicResource GeneralButtonStyle}" Content="LOGIN" Height="32" Click="BtnClientLogin_Click" >
                            </Button>
                        </Grid>
                    </Grid>


                    <Border Grid.Row="8" Style="{DynamicResource BlueBorder}">
                        <TextBlock Style="{DynamicResource BlueBorderInfo}" Text="Export Existing Master Data to XEPOS Host"/>
                    </Border>
                    <Border Style="{DynamicResource DummyBorder}" Grid.Row="9" Grid.RowSpan="2" x:Name="gcco4">
                        <Grid Margin="9" >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="15"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="10"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                            </Grid.RowDefinitions>


                            <TextBlock Grid.Row="0" TextWrapping="WrapWithOverflow" Text="Export store Departments, POS Screen Categories, Item families,Taxes, and Items to XEPOS Host. This is an one time process and process to setup XEPOS Host. It should be used if individul stores are operational before XEPOS Host is set up."></TextBlock>

                            <Button Grid.Row="2" Grid.Column="2"  x:Name="btnExportMasterData" Style="{DynamicResource GeneralButtonStyle}" Content="EXPORT" Height="40" Click="BtnExportMasterData_Click" >
                            </Button>

                            

                        </Grid>
                    </Border>

                    <TextBlock Grid.Row="12" x:Name="txtExportStatus" Text="" Style="{DynamicResource LoginUser}" FontSize="15" TextWrapping="WrapWithOverflow"></TextBlock>


                </Grid>



                <Grid Grid.Column="2" Grid.Row="0" Grid.RowSpan="2">

                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"></RowDefinition>

                        <RowDefinition Height="*"></RowDefinition>
                    </Grid.RowDefinitions>

                    <Border Style="{DynamicResource PurpleBorder}">
                        <TextBlock Style="{DynamicResource PurpleBorderInfo}" Text="Task Seheduler Setup"/>
                    </Border>

                    <Border Grid.Row="1" x:Name="gcco2">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="9"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="15"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="9"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                            </Grid.RowDefinitions>

                            <TextBlock Grid.Row="0"  Text="Export Scheduler" Style="{DynamicResource LoginUser}"></TextBlock>
                            <Rectangle Grid.Row="1" Style="{DynamicResource DefaultLineStyle}"  VerticalAlignment="Center" />
                            <Grid Grid.Row="2">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="236*"/>
                                    <ColumnDefinition Width="221*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="9"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="9"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="9"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="9"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>

                                </Grid.RowDefinitions>


                                <TextBlock Grid.Row="0" TextWrapping="WrapWithOverflow" Text="For sending sales, inventory, closeout, purchase order, Receiving  &amp; attendace data to XEPOS Host"  Grid.RowSpan="2"  Grid.ColumnSpan="2"></TextBlock>
                                <TextBlock Grid.Row="1" Text="Daily on"  Grid.RowSpan="3" Grid.ColumnSpan="2"  Margin="0,24,0,20"></TextBlock>
                                <Grid Grid.Row="3" Grid.ColumnSpan="2" Grid.RowSpan="2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                                        <ColumnDefinition Width="15"></ColumnDefinition>
                                        <ColumnDefinition Width="1.2*"></ColumnDefinition>
                                        <ColumnDefinition Width="5"></ColumnDefinition>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>

                                    <dxe:DateEdit Grid.Column="0" Width="130" HorizontalAlignment="Left" Style="{DynamicResource SpinDate}" x:Name="txtTime" Mask="hh:mm tt" MaskType="DateTime" MaskUseAsDisplayFormat="True" AllowDefaultButton="False"  Height="32">
                                        <dxe:DateEdit.Buttons>
                                            <dxe:SpinButtonInfo IsDefaultButton="True" />
                                        </dxe:DateEdit.Buttons>
                                    </dxe:DateEdit>

                                    <Button Grid.Column="2"  Width="160"  x:Name="btnSchedule" Style="{DynamicResource GeneralButtonStyle}" Content="SET TASK SCHEDULER" FontSize="11" Tag="Add Task Scheduler"  Click="BtnSchedule_Click" >

                                    </Button>

                                    <Button Grid.Column="4"     Width="130"  x:Name="btnRunSchedule" Style="{DynamicResource GeneralButtonStyle}" Content="RUN SCHEDULER" FontSize="11"  Visibility="Hidden" Click="BtnRunSchedule_Click" >
                                    </Button>

                                </Grid>




                                <TextBlock Grid.Row="5" Grid.ColumnSpan="2" Text="For sending customer, gift certificate updates to XEPOS Host" Margin="0,8,0,0" Grid.RowSpan="2"></TextBlock>
                                <TextBlock Grid.Row="8" Grid.Column="0" Text="Daily on" Grid.ColumnSpan="2" Margin="0,0,0,2" Grid.RowSpan="2"></TextBlock>

                                <Grid Grid.Row="10" Grid.ColumnSpan="2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                                        <ColumnDefinition Width="15"></ColumnDefinition>
                                        <ColumnDefinition Width="1.2*"></ColumnDefinition>
                                        <ColumnDefinition Width="5"></ColumnDefinition>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>

                                    <dxe:DateEdit Grid.Column="0" Width="130" HorizontalAlignment="Left" Style="{DynamicResource SpinDate}" x:Name="txtgcTime" Mask="hh:mm tt" MaskType="DateTime" MaskUseAsDisplayFormat="True" AllowDefaultButton="False" >
                                        <dxe:DateEdit.Buttons>
                                            <dxe:SpinButtonInfo IsDefaultButton="True" />
                                        </dxe:DateEdit.Buttons>
                                    </dxe:DateEdit>
                                    <Button Grid.Column="2"  Width="160"  x:Name="btnGCSchedule" Style="{DynamicResource GeneralButtonStyle}" Content="SET TASK SCHEDULER"  Tag="Add Task Scheduler" FontSize="11"  Click="BtnGCSchedule_Click" >
                                    </Button>
                                    <Button Grid.Column="4"  Width="130"   x:Name="btnGCRunSchedule" Style="{DynamicResource GeneralButtonStyle}" Content="RUN SCHEDULER"   Visibility="Hidden" FontSize="11" Click="BtnGCRunSchedule_Click" >
                                    </Button>

                                </Grid>

                                <dxe:ToggleSwitch ContentPlacement="Far" HorizontalAlignment="Left" Grid.Row="12" Grid.Column="0" CheckedStateContent="Repeat task" UncheckedStateContent="Repeat task" x:Name="chkgcRepeat" Checked="ChkgcRepeat_Checked" Unchecked="ChkgcRepeat_Checked" />
                                <Grid Grid.Row="14"  Margin="9,0" Grid.ColumnSpan="2" >
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"></ColumnDefinition>
                                            <ColumnDefinition Width="9"></ColumnDefinition>
                                            <ColumnDefinition Width="*"></ColumnDefinition>
                                            <ColumnDefinition Width="9"></ColumnDefinition>
                                            <ColumnDefinition Width="*"></ColumnDefinition>
                                            <ColumnDefinition Width="9"></ColumnDefinition>
                                            <ColumnDefinition Width="*"></ColumnDefinition>
                                            <ColumnDefinition Width="9"></ColumnDefinition>
                                            <ColumnDefinition Width="*"></ColumnDefinition>

                                        </Grid.ColumnDefinitions>

                                        <TextBlock Grid.Column="0" Text="Every" x:Name="rep4" HorizontalAlignment="Center" Visibility="Hidden"/>
                                        <dxe:SpinEdit Grid.Column="2" x:Name="rep1" Text="10" Mask="N0" MaskType="Numeric" MinValue="1" MaxValue="60"  Visibility="Hidden" HorizontalContentAlignment="Center">
                                            <dxe:SpinEdit.Buttons>
                                                <dxe:ButtonInfoCollection>
                                                    <dxe:ButtonInfo Command="{Binding SpinDownCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=dxe:SpinEdit}}">
                                                        <dxe:ButtonInfo.Content>
                                                            <dx:DXImage Source="{DynamicResource GreenDropdownD}" Height="15" Width="16"  Margin="5,5">
                                                            </dx:DXImage>
                                                        </dxe:ButtonInfo.Content>
                                                    </dxe:ButtonInfo>
                                                    <dxe:ButtonInfo Command="{Binding SpinUpCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=dxe:SpinEdit}}" IsLeft="True">
                                                        <dxe:ButtonInfo.Content>
                                                            <dx:DXImage Source="{DynamicResource GreenDropdownU}" Height="15" Width="16"  Margin="5,5">
                                                            </dx:DXImage>
                                                        </dxe:ButtonInfo.Content>
                                                    </dxe:ButtonInfo>
                                                </dxe:ButtonInfoCollection>
                                            </dxe:SpinEdit.Buttons>
                                        </dxe:SpinEdit>
                                        <dxe:ComboBoxEdit Grid.Column="4" x:Name="rep2"  Visibility="Hidden" SelectedIndex="0" PreviewMouseLeftButtonDown="Rep2_PreviewMouseLeftButtonDown">
                                            <dxe:ComboBoxEditItem>minutes</dxe:ComboBoxEditItem>
                                            <dxe:ComboBoxEditItem>hours</dxe:ComboBoxEditItem>
                                        </dxe:ComboBoxEdit>
                                        <TextBlock Grid.Column="6" Text="Until" x:Name="rep5" HorizontalAlignment="Center"  Visibility="Hidden"/>
                                        <dxe:DateEdit Mask="hh:mm tt" Style="{DynamicResource SpinDate}" MaskType="DateTime" MaskUseAsDisplayFormat="True" AllowDefaultButton="False"  Grid.Column="8" x:Name="rep3"  Visibility="Hidden">
                                            <dxe:DateEdit.Buttons>
                                                <dxe:SpinButtonInfo IsDefaultButton="True" />
                                            </dxe:DateEdit.Buttons>
                                        </dxe:DateEdit>
                                    </Grid>
                                </Grid>
                            </Grid>


                            <TextBlock Grid.Row="4"  Text="Import Scheduler" Style="{DynamicResource LoginUser}"></TextBlock>
                            <Rectangle Grid.Row="5" Style="{DynamicResource DefaultLineStyle}"  VerticalAlignment="Center" />
                            <Grid Grid.Row="6">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="9"></RowDefinition>
                                    <RowDefinition Height="20"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="9"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="9"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="9"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="9"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                </Grid.RowDefinitions>

                                <TextBlock Grid.Row="0" TextWrapping="WrapWithOverflow"  Text="For receiving sales,inventory,discounts,promotions &amp; pricing data from XEPOS Host"  Grid.RowSpan="2" />
                                <TextBlock Grid.Row="2"  Text="Daily on" ></TextBlock>

                                <Grid Grid.Row="4">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                                        <ColumnDefinition Width="15"></ColumnDefinition>
                                        <ColumnDefinition Width="1.2*"></ColumnDefinition>
                                        <ColumnDefinition Width="5"></ColumnDefinition>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>

                                    <dxe:DateEdit Grid.Column="0" Width="130"  Style="{DynamicResource SpinDate}" x:Name="txtITime" Mask="hh:mm tt" MaskType="DateTime" MaskUseAsDisplayFormat="True" AllowDefaultButton="False"  >
                                        <dxe:DateEdit.Buttons>
                                            <dxe:SpinButtonInfo IsDefaultButton="True" />
                                        </dxe:DateEdit.Buttons>
                                    </dxe:DateEdit>
                                    <Button Grid.Column="2" Width="160"   x:Name="btnISchedule" Style="{DynamicResource GeneralButtonStyle}"  Tag="Add Task Scheduler" FontSize="11" Content="SET TASK SCHEDULER"  Click="BtnISchedule_Click" >
                                    </Button>
                                    <Button Grid.Column="4" Width="130"  x:Name="btnIRunSchedule" Style="{DynamicResource GeneralButtonStyle}" Content="RUN SCHEDULER" FontSize="11"  Visibility="Hidden" Click="BtnIRunSchedule_Click" >
                                    </Button>

                                </Grid>


                                <dxe:ToggleSwitch ContentPlacement="Far" HorizontalAlignment="Left" Grid.Row="6" Grid.Column="0"  CheckedStateContent="Run controller on Scale Com" UncheckedStateContent="Run controller on Scale Com"  x:Name="chkRunScaleComController"  />


                                <TextBlock Grid.Row="8" Grid.ColumnSpan="5" Text="For receiving customer, gift certificate updates from XEPOS Host"></TextBlock>
                                <TextBlock Grid.Row="10" Grid.Column="0" Text="Daily on"></TextBlock>

                                <Grid Grid.Row="12">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                                        <ColumnDefinition Width="15"></ColumnDefinition>
                                        <ColumnDefinition Width="1.2*"></ColumnDefinition>
                                        <ColumnDefinition Width="5"></ColumnDefinition>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <dxe:DateEdit Width="130"  Grid.Column="0" Style="{DynamicResource SpinDate}" x:Name="txtIgcTime" Mask="hh:mm tt" MaskType="DateTime" MaskUseAsDisplayFormat="True" AllowDefaultButton="False"  Height="32">
                                        <dxe:DateEdit.Buttons>
                                            <dxe:SpinButtonInfo IsDefaultButton="True" />
                                        </dxe:DateEdit.Buttons>
                                    </dxe:DateEdit>
                                    <Button  Grid.Column="2" Width="160"   x:Name="btnIGCSchedule" Style="{DynamicResource GeneralButtonStyle}"  Tag="Add Task Scheduler" Content="SET TASK SCHEDULER" FontSize="11"  Click="BtnIGCSchedule_Click" >
                                    </Button>
                                    <Button  Grid.Column="4" Width="130"  x:Name="btnIGCRunSchedule" Style="{DynamicResource GeneralButtonStyle}" Content="RUN SCHEDULER"  FontSize="11"  Visibility="Hidden" Click="BtnIGCRunSchedule_Click" >
                                    </Button>
                                </Grid>

                                <dxe:ToggleSwitch ContentPlacement="Far" HorizontalAlignment="Left" Grid.Row="14" Grid.Column="0" CheckedStateContent="Repeat task" UncheckedStateContent="Repeat task" x:Name="chkIgcRepeat" Checked="ChkIgcRepeat_Checked" Unchecked="ChkIgcRepeat_Checked" />
                                <Grid Grid.Row="16" Margin="9,0">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"></ColumnDefinition>
                                            <ColumnDefinition Width="9"></ColumnDefinition>
                                            <ColumnDefinition Width="*"></ColumnDefinition>
                                            <ColumnDefinition Width="9"></ColumnDefinition>
                                            <ColumnDefinition Width="*"></ColumnDefinition>
                                            <ColumnDefinition Width="9"></ColumnDefinition>
                                            <ColumnDefinition Width="*"></ColumnDefinition>
                                            <ColumnDefinition Width="9"></ColumnDefinition>
                                            <ColumnDefinition Width="*"></ColumnDefinition>

                                        </Grid.ColumnDefinitions>

                                        <TextBlock Grid.Column="0" Text="Every" x:Name="repi4" HorizontalAlignment="Center" Visibility="Hidden"/>
                                        <dxe:SpinEdit Grid.Column="2" x:Name="repi1" Text="1" Mask="N0" MaskType="Numeric" MinValue="1" MaxValue="60" Visibility="Hidden" HorizontalContentAlignment="Left">
                                            <dxe:SpinEdit.Buttons>
                                                <dxe:ButtonInfoCollection>
                                                    <dxe:ButtonInfo Command="{Binding SpinDownCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=dxe:SpinEdit}}">
                                                        <dxe:ButtonInfo.Content>
                                                            <dx:DXImage Source="{DynamicResource GreenDropdownD}" Height="15" Width="16"  Margin="5,5">
                                                            </dx:DXImage>
                                                        </dxe:ButtonInfo.Content>
                                                    </dxe:ButtonInfo>
                                                    <dxe:ButtonInfo Command="{Binding SpinUpCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=dxe:SpinEdit}}" IsLeft="True">
                                                        <dxe:ButtonInfo.Content>
                                                            <dx:DXImage Source="{DynamicResource GreenDropdownU}" Height="15" Width="16"  Margin="5,5">
                                                            </dx:DXImage>
                                                        </dxe:ButtonInfo.Content>
                                                    </dxe:ButtonInfo>
                                                </dxe:ButtonInfoCollection>
                                            </dxe:SpinEdit.Buttons>
                                        </dxe:SpinEdit>
                                        <dxe:ComboBoxEdit Grid.Column="4" x:Name="repi2"  Visibility="Hidden" SelectedIndex="1" PreviewMouseLeftButtonDown="Rep2_PreviewMouseLeftButtonDown">
                                            <dxe:ComboBoxEditItem>minutes</dxe:ComboBoxEditItem>
                                            <dxe:ComboBoxEditItem>hours</dxe:ComboBoxEditItem>
                                        </dxe:ComboBoxEdit>
                                        <TextBlock Grid.Column="6" Text="Until" x:Name="repi5" HorizontalAlignment="Center" Visibility="Hidden"/>
                                        <dxe:DateEdit Mask="hh:mm tt" Style="{DynamicResource SpinDate}" MaskType="DateTime" MaskUseAsDisplayFormat="True" AllowDefaultButton="False"  Grid.Column="8" x:Name="repi3"  Visibility="Hidden">
                                            <dxe:DateEdit.Buttons>
                                                <dxe:SpinButtonInfo IsDefaultButton="True" />
                                            </dxe:DateEdit.Buttons>
                                        </dxe:DateEdit>
                                    </Grid>
                                </Grid>
                            </Grid>

                        </Grid>
                    </Border>

                </Grid>


                <!--
                <GroupBox Header="Store Setup" Grid.Row="1" Grid.Column="0" x:Name="gcco1">
                        <Grid Margin="9">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="9"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
