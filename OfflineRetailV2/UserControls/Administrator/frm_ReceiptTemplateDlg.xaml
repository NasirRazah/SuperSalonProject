                        <Button Grid.Row="1" Content="Delete" HorizontalAlignment="Left" HorizontalContentAlignment="Left" x:Name="popupDelete" Style="{DynamicResource RTPopupButtonStyle}" Click="PopupDelete_Click"></Button>
                        
                    </Grid> 
