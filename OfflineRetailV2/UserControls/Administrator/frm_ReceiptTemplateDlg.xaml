                        Style="{DynamicResource SaveButtonStyle}" x:Name="btnSubmitL" Click="BtnSubmitL_Click" />
                                </Grid>

                            </Grid>

                            <Grid Grid.Row="1" x:Name="grdEditLabelBarcode" Visibility="Collapsed">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"></RowDefinition>
                                    <RowDefinition Height="3"></RowDefinition>
