            x:Name="flyoutControl"
            FontSize="16"
            AllowMoveAnimation="False"
            FontFamily="Segoe UI Light"
            IsOpen="False">
                    <Grid >
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                        </Grid.RowDefinitions>
