<Window xmlns:dxp="http://schemas.devexpress.com/winfx/2008/xaml/printing"  x:Class="OfflineRetailV2.UserControls.Administrator.frm_ReceiptTemplateDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OfflineRetailV2.UserControls.Administrator"
              xmlns:cc="clr-namespace:OfflineRetailV2.Controls"
        
        xmlns:lm="clr-namespace:OfflineRetailV2.LayoutManager"
        xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
        xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid" 
        xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
        xmlns:dxpbars="http://schemas.devexpress.com/winfx/2008/xaml/printing/bars"
        AllowsTransparency="True" WindowStyle="None" Background="Transparent" WindowStartupLocation="CenterScreen"
        Title="" Foreground="White"  
              Width="800" Height="600" Closing="Window_Closing" Loaded="Window_Loaded">
    <cc:ModalWindow  x:Name="ModalWindow">
        <Grid Margin="9">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="9"/>
                <RowDefinition Height="*"/>

            </Grid.RowDefinitions>
            <Grid Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                    <ColumnDefinition Width="Auto"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <TextBlock Text="Title"  x:Name="lbTitle" VerticalAlignment="Center" Style="{DynamicResource BlueHeading}" />
                <Grid Grid.Column="1" HorizontalAlignment="Right" Margin="0,0,20,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                        <ColumnDefinition Width="9"></ColumnDefinition>
                        <ColumnDefinition Width="Auto"></ColumnDefinition>
                    </Grid.ColumnDefinitions>

                    <Button Grid.Column="0" Content="CANCEL" Height="35" Width="120" FontFamily="{StaticResource OSSemiBold}" 
                        Style="{DynamicResource CancelButtonStyle}" x:Name="btnCancel" Click="BtnCancel_Click" />
                    <Button Grid.Column="2" Content="SAVE" Height="35" Width="120" FontFamily="{StaticResource OSSemiBold}" 
                        Style="{DynamicResource SaveButtonStyle}" x:Name="btnLogin" Click="BtnLogin_Click"/>
                </Grid>
            </Grid>
            <Rectangle Grid.Row="1" Style="{DynamicResource DefaultLineStyle}"  VerticalAlignment="Center"  Margin="-9,0"/>

            <Grid Grid.Row="2" Margin="0,9,0,0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                    <ColumnDefinition Width="10"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid Grid.Column="0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="3"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="3"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="3"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="3"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="3"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="3"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="3"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="15"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                        <RowDefinition Height="3"></RowDefinition>
                        <RowDefinition Height="Auto"></RowDefinition>
                    </Grid.RowDefinitions>

                    <TextBlock Grid.Row="0" Text="Template Type"/>
                    <dxe:ComboBoxEdit Grid.Row="2"  x:Name="txtName" IsTextEditable="False" ItemsSource="{Binding}" SelectedIndexChanged="TxtName_SelectedIndexChanged" EditValueChanged="TxtName_EditValueChanged" PreviewMouseLeftButtonDown="TxtName_PreviewMouseLeftButtonDown"/>

                    <TextBlock Grid.Row="4" Text="Template Name"/>

                    <dxe:TextEdit Grid.Row="6"  x:Name="txtTemplateName" MaxLength="50"  GotFocus="Full_GotFocus" LostFocus="Full_LostFocus"/>

                    <TextBlock Grid.Row="8" Text="Template Size" x:Name="lbSize"/>
                    <Grid Grid.Row="10" x:Name="pnlTemplateSizeNormal">
                        <dxe:ComboBoxEdit HorizontalAlignment="Stretch"
                                          x:Name="cmbTemplateSize" SelectedIndex="0" IsTextEditable="False"  PreviewMouseLeftButtonDown="CmbAlign_PreviewMouseLeftButtonDown">
                            <dxe:ComboBoxEditItem>80 mm</dxe:ComboBoxEditItem>
                            <dxe:ComboBoxEditItem>58 mm</dxe:ComboBoxEditItem>
                        </dxe:ComboBoxEdit>
                    </Grid>

                    <Grid Grid.Row="10" x:Name="pnlTemplateSizeLabel" Visibility="Hidden">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                            <ColumnDefinition Width="3"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="3"></ColumnDefinition>
                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                            <ColumnDefinition Width="3"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>

                        <TextBlock Grid.Column="0" Text="Width"/>

                        <dxe:SpinEdit Grid.Column="2"  HorizontalContentAlignment="Center" x:Name="txtTemplateW" Text="280" Mask="N00" MaskType="Numeric" MaskUseAsDisplayFormat="True" MinValue="100" Increment="1" >
                            <dxe:SpinEdit.Buttons>
                                <dxe:ButtonInfoCollection>
                                    <dxe:ButtonInfo Command="{Binding SpinDownCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=dxe:SpinEdit}}">
                                        <dxe:ButtonInfo.Content>
                                            <dx:DXImage Source="{DynamicResource GreenDropdownD}" Height="15" Width="16"  Margin="5,5">
                                            </dx:DXImage>
                                        </dxe:ButtonInfo.Content>
                                    </dxe:ButtonInfo>
                                    <dxe:ButtonInfo Command="{Binding SpinUpCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=dxe:SpinEdit}}" IsLeft="True">
                                        <dxe:ButtonInfo.Content>
                                            <dx:DXImage Source="{DynamicResource GreenDropdownU}" Height="15" Width="16"  Margin="5,5">
                                            </dx:DXImage>
                                        </dxe:ButtonInfo.Content>
                                    </dxe:ButtonInfo>
                                </dxe:ButtonInfoCollection>
                            </dxe:SpinEdit.Buttons>
                        </dxe:SpinEdit>

                        <TextBlock Grid.Column="4" Text="Height"/>


                        <dxe:SpinEdit Grid.Column="6"  HorizontalContentAlignment="Center" x:Name="txtTemplateH" Text="125" Mask="N00" MaskType="Numeric" MaskUseAsDisplayFormat="True" MinValue="50" Increment="1" >
                            <dxe:SpinEdit.Buttons>
                                <dxe:ButtonInfoCollection>
                                    <dxe:ButtonInfo Command="{Binding SpinDownCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=dxe:SpinEdit}}">
                                        <dxe:ButtonInfo.Content>
                                            <dx:DXImage Source="{DynamicResource GreenDropdownD}" Height="15" Width="16"  Margin="5,5">
                                            </dx:DXImage>
                                        </dxe:ButtonInfo.Content>
                                    </dxe:ButtonInfo>
                                    <dxe:ButtonInfo Command="{Binding SpinUpCommand, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=dxe:SpinEdit}}" IsLeft="True">
                                        <dxe:ButtonInfo.Content>
                                            <dx:DXImage Source="{DynamicResource GreenDropdownU}" Height="15" Width="16"  Margin="5,5">
                                            </dx:DXImage>
                                        </dxe:ButtonInfo.Content>
                                    </dxe:ButtonInfo>
                                </dxe:ButtonInfoCollection>
                            </dxe:SpinEdit.Buttons>
                        </dxe:SpinEdit>

                    </Grid>


                    <TextBlock Grid.Row="12" Text="Number of Copies" x:Name="lbPrintCopy"  />
                    <dxe:ComboBoxEdit Grid.Row="14" x:Name="cmbPrintCopy" SelectedIndex="0" IsTextEditable="False" PreviewMouseLeftButtonDown="CmbPrintCopy_PreviewMouseLeftButtonDown">
                        <dxe:ComboBoxEditItem>One Copy</dxe:ComboBoxEditItem>
                        <dxe:ComboBoxEditItem>Two Copy</dxe:ComboBoxEditItem>
                        <dxe:ComboBoxEditItem>Three Copy</dxe:ComboBoxEditItem>
                    </dxe:ComboBoxEdit>

                    <dxe:ToggleSwitch Grid.Row="16"  x:Name="chkEditTemplate" ContentPlacement="Far" CheckedStateContent="Personalize Template" UncheckedStateContent="Personalize Template" HorizontalAlignment="Left" IsChecked="False"  Checked="ChkEditTemplate_Checked" Unchecked="ChkEditTemplate_Checked"></dxe:ToggleSwitch>


                   
                    
                </Grid>
                
                
                

                
                <dxe:FlyoutControl
